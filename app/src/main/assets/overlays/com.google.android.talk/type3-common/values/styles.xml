<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="EsActionBar" parent="@*android:style/Widget.Holo.ActionBar.Solid">
        <item name="android:background">@*com.google.android.talk:color/primary</item>
        <item name="android:logo">@*android:color/transparent</item>
        <item name="android:titleTextStyle">@*com.google.android.talk:style/EsActionBar.Title</item>
    </style>
    <style name="Hangouts.Fauxbar" parent="@*com.google.android.talk:style/Hangouts.Snackbar">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:minHeight">56.0dip</item>
        <item name="*com.google.android.talk:messageTextColor">@*android:color/white</item>
        <item name="*com.google.android.talk:actionTextColor">@*com.google.android.talk:color/primary</item>
    </style>
    <style name="InCallMessage.Name" parent="@*com.google.android.talk:style/Theme.EmeraldSea">
        <item name="android:textSize">@*com.google.android.talk:dimen/in_call_message_name_size</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:singleLine">true</item>
        <item name="android:alpha">1.00</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="InCallMessage.Text" parent="@*com.google.android.talk:style/Theme.EmeraldSea">
        <item name="android:textSize">@*com.google.android.talk:dimen/in_call_message_text_size</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:textColorLink">@*com.google.android.talk:color/in_call_message_link</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:singleLine">false</item>
        <item name="android:alpha">1.00</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="LastReadIndicatorLineViewStyle">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:background">@*android:color/white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@*com.google.android.talk:dimen/last_read_message_line_height</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="LastReadIndicatorMessageTextStyle">
        <item name="android:textSize">@*com.google.android.talk:dimen/last_read_message_text_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@*com.google.android.talk:dimen/last_read_message_text_margin</item>
        <item name="android:layout_marginRight">@*com.google.android.talk:dimen/last_read_message_text_margin</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:layout_marginStart">@*com.google.android.talk:dimen/last_read_message_text_margin</item>
        <item name="android:layout_marginEnd">@*com.google.android.talk:dimen/last_read_message_text_margin</item>
    </style>
    <style name="PeopleListItemViewNameStyle">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">@*com.google.android.talk:integer/appcompat_sans_serif_medium_textStyle</item>
        <item name="android:textColor">@*com.google.android.talk:color/people_name</item>
        <item name="android:shadowColor">@*android:color/white</item>
        <item name="android:shadowDx">1.5</item>
        <item name="android:shadowDy">1.3</item>
        <item name="android:shadowRadius">8.0</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
        <item name="android:fontFamily">@*com.google.android.talk:string/appcompat_sans_serif_medium_fontFamily</item>
    </style>
    <style name="Platform.V21.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorHintInverse">#ffbbbbbb</item>
        <item name="android:textColorHint">#ffbbbbbb</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
        <item name="android:textColorLinkInverse">@*android:color/accent_material_dark</item>
    </style>
    <style name="SettingsActionBarStyle" parent="@*android:style/Widget.Material.ActionBar">
        <item name="android:icon">@*android:color/transparent</item>
        <item name="android:background">@*com.google.android.talk:color/settings_action_bar_background_color</item>
        <item name="android:displayOptions">homeAsUp|showHome|showTitle</item>
        <item name="android:contentInsetStart">@*com.google.android.talk:dimen/settings_action_bar_content_inset_start</item>
    </style>
    <style name="SettingsStyle" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:actionBarStyle">@*com.google.android.talk:style/SettingsActionBarStyle</item>
    </style>
    <style name="Theme.AlertDialog" parent="@*android:style/Theme.Material.Dialog.Alert" />
    <style name="Theme.CallerId" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorPrimary">@*android:color/white</item>
        <item name="android:textColor">@*android:color/secondary_text_default_material_dark</item>
        <item name="android:colorPrimary">@*com.google.android.talk:color/primary</item>
        <item name="android:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="android:colorAccent">@*com.google.android.talk:color/primary</item>
    </style>
    <style name="Theme.DialerPromo" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:background">@*com.google.android.talk:color/primary</item>
        <item name="android:colorPrimaryDark">@*com.google.android.talk:color/primary_dark</item>
    </style>
    <style name="Theme.EmeraldSea.Launcher" parent="@*com.google.android.talk:style/Theme.EmeraldSea">	
        <item name="android:windowBackground">@*com.google.android.talk:drawable/quantum_launchscreen_hangouts</item>
        <item name="*com.google.android.talk:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
	    <item name="android:windowShowWallpaper">false</item>		
        <item name="android:windowIsTranslucent">false</item>
    </style>
    <style name="Theme.EmeraldSea.Settings" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:actionBarStyle">@*com.google.android.talk:style/EsActionBar</item>
        <item name="android:homeAsUpIndicator">@*com.google.android.talk:drawable/ic_arrow_back_black</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@*com.google.android.talk:color/primary_dark</item>
    </style>
    <style name="Theme.Hangout" parent="@*com.google.android.talk:style/Theme.EmeraldSea">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="*com.google.android.talk:actionOverflowButtonStyle">@*com.google.android.talk:style/Widget.AppCompat.ActionButton.Overflow</item>
        <item name="*com.google.android.talk:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="Theme.Hangout.IncomingRing" parent="@*android:style/Theme.Holo.DialogWhenLarge.NoActionBar">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="Theme.Hangouts.Settings" parent="@*com.google.android.talk:style/Theme.AppCompat">
        <item name="android:textColorPrimary">@*com.google.android.talk:color/text_color_primary_settings</item>
        <item name="android:textColorSecondary">@*com.google.android.talk:color/text_color_secondary_settings</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:dividerHeight">@*com.google.android.talk:dimen/divider_height_settings</item>
        <item name="android:listDivider">@*com.google.android.talk:drawable/list_divider_settings</item>
        <item name="android:actionBarStyle">@*com.google.android.talk:style/Hangout.Settings.ActionBar</item>
        <item name="android:colorAccent">@*com.google.android.talk:color/accent</item>
        <item name="*com.google.android.talk:actionOverflowButtonStyle">@*com.google.android.talk:style/ActionBarOverflow</item>
        <item name="*com.google.android.talk:colorAccent">@*com.google.android.talk:color/accent</item>
        <item name="*com.google.android.talk:colorPrimary">@*com.google.android.talk:color/primary</item>
        <item name="*com.google.android.talk:colorPrimaryDark">@*com.google.android.talk:color/primary_dark</item>
        <item name="*com.google.android.talk:supportCheckBoxPreferenceStyle">@*com.google.android.talk:style/SupportPreference.CheckBoxPreference</item>
        <item name="*com.google.android.talk:supportDialogPreferenceStyle">@*com.google.android.talk:style/SupportPreference.DialogPreference</item>
        <item name="*com.google.android.talk:supportEditTextPreferenceStyle">@*com.google.android.talk:style/SupportPreference.DialogPreference.EditTextPreference</item>
        <item name="*com.google.android.talk:supportPreferenceCategoryStyle">@*com.google.android.talk:style/SupportPreference.Category</item>
        <item name="*com.google.android.talk:supportPreferenceFragmentStyle">@*com.google.android.talk:style/SupportPreferenceFragment</item>
        <item name="*com.google.android.talk:supportPreferenceInformationStyle">@*com.google.android.talk:style/SupportPreference.Information</item>
        <item name="*com.google.android.talk:supportPreferencePanelStyle">@*com.google.android.talk:style/SupportPreferencePanel</item>
        <item name="*com.google.android.talk:supportPreferenceScreenStyle">@*com.google.android.talk:style/SupportPreference.PreferenceScreen</item>
        <item name="*com.google.android.talk:supportPreferenceStyle">@*com.google.android.talk:style/SupportPreference</item>
        <item name="*com.google.android.talk:supportRingtonePreferenceStyle">@*com.google.android.talk:style/SupportPreference.RingtonePreference</item>
        <item name="*com.google.android.talk:supportSwitchPreferenceStyle">@*com.google.android.talk:style/SupportPreference.SwitchPreference</item>
        <item name="*com.google.android.talk:supportYesNoPreferenceStyle">@*com.google.android.talk:style/SupportPreference.DialogPreference.YesNoPreference</item>
    </style>
    <style name="Theme.Hangouts.White" parent="@*com.google.android.talk:style/Theme.EmeraldSea">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="*com.google.android.talk:windowActionBar">false</item>
        <item name="*com.google.android.talk:windowNoTitle">true</item>
        <item name="*com.google.android.talk:actionOverflowButtonStyle">@*com.google.android.talk:style/Widget.AppCompat.ActionButton.Overflow</item>
    </style>
    <style name="Theme.InCallDialpad" parent="@*android:style/Theme.Holo.Dialog.NoActionBar" />
    <style name="Theme.PhoneVerification" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorPrimary">@*android:color/white</item>
        <item name="android:textColor">@*android:color/secondary_text_default_material_dark</item>
        <item name="android:colorPrimary">@*com.google.android.talk:color/primary</item>
        <item name="android:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="android:colorAccent">@*com.google.android.talk:color/primary</item>
    </style>
    <style name="Theme.Promo" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorPrimary">@*android:color/white</item>
        <item name="android:textColor">@*android:color/secondary_text_default_material_dark</item>
        <item name="android:colorPrimary">@*com.google.android.talk:color/primary</item>
        <item name="android:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="android:colorAccent">@*com.google.android.talk:color/primary</item>
    </style>
</resources>
