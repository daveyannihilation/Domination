<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <inset android:insetTop="6.0dip" android:insetBottom="6.0dip">
            <shape android:shape="rectangle">
                <corners android:radius="@*com.google.android.talk:dimen/button_corner_radius" />
                <solid android:color="#ff555555" />
            </shape>
        </inset>
    </item>
    <item android:state_focused="true">
        <inset android:insetTop="6.0dip" android:insetBottom="6.0dip">
            <shape android:shape="rectangle">
                <corners android:radius="@*com.google.android.talk:dimen/button_corner_radius" />
                <solid android:color="#ff555555" />
            </shape>
        </inset>
    </item>
    <item android:state_hovered="true">
        <inset android:insetTop="6.0dip" android:insetBottom="6.0dip">
            <shape android:shape="rectangle">
                <corners android:radius="@*com.google.android.talk:dimen/button_corner_radius" />
                <solid android:color="#ff555555" />
            </shape>
        </inset>
    </item>
    <item>
        <inset android:insetTop="6.0dip" android:insetBottom="6.0dip">
            <shape android:shape="rectangle">
                <corners android:radius="@*com.google.android.talk:dimen/button_corner_radius" />
                <solid android:color="@*android:color/button_material_dark" />
            </shape>
        </inset>
    </item>
</selector>
