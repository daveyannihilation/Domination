<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Base.TextAppearance.AppCompat.Inverse" parent="@*android:style/TextAppearance.Material" />
    <style name="Base.TextAppearance.AppCompat.Large.Inverse" parent="@*android:style/TextAppearance.Material.Large" />
    <style name="Base.TextAppearance.AppCompat.Medium.Inverse" parent="@*android:style/TextAppearance.Material.Medium" />
    <style name="Base.TextAppearance.AppCompat.Small.Inverse" parent="@*android:style/TextAppearance.Material.Small" />
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabText" parent="@*android:style/Widget.Material.ActionBar.TabText" />
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabView" parent="@*android:style/Widget.Material.ActionBar.TabView" />
    <style name="Base.Widget.AppCompat.Light.PopupMenu" parent="@*android:style/Widget.Material.PopupMenu" />
    <style name="BaseActionBarOverflow" parent="@*android:style/Widget.Material.ActionButton.Overflow" />
    <style name="BaseAppTheme" parent="@*android:style/Theme.Material">
        <item name="android:buttonStyle">@*com.google.android.apps.docs:style/BaseButton</item>
        <item name="android:windowContentTransitions">true</item>
        <item name="android:colorPrimary">@*android:color/secondary_text_leanback_light</item>
        <item name="android:colorPrimaryDark">@*android:color/primary_text_leanback_light</item>
        <item name="android:colorAccent">#ff1976d2</item>
        <item name="android:windowShowWallpaper">false</item>
    </style>
    <style name="BaseAutoCompleteTextView" parent="@*android:style/Widget.Material.AutoCompleteTextView" />
    <style name="BaseDialogTheme" parent="@*android:style/Theme.Material.Dialog.MinWidth">
        <item name="android:colorAccent">#ff1976d2</item>
    </style>
    <style name="BaseDialogThemeNoFrame" parent="@*android:style/Theme.Material.Dialog.MinWidth">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowCloseOnTouchOutside">false</item>
        <item name="android:colorAccent">#ff1976d2</item>
        <item name="android:windowElevation">0.0dip</item>
    </style>
    <style name="BaseDialogThemeWhenLarge" parent="@*android:style/Theme.Material.DialogWhenLarge">
        <item name="android:colorAccent">#ff1976d2</item>
    </style>
    <style name="CakemixTheme.Projector" parent="@*android:style/Theme.Material">
        <item name="android:actionBarStyle">@*com.google.android.apps.docs:style/ProjectorActionBar</item>
        <item name="android:windowShowWallpaper">false</item>
    </style>
    <style name="Platform.V21.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorHintInverse">#ffbbbbbb</item>
        <item name="android:textColorHint">#ffbbbbbb</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
        <item name="android:textColorLinkInverse">@*android:color/accent_material_dark</item>
        <item name="android:windowShowWallpaper">false</item>
    </style>
    <style name="ProgressBar.Horizontal" parent="@*android:style/Widget.Material.ProgressBar.Horizontal">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateBehavior">repeat</item>
    </style>
    <style name="ProgressBarSpinner" parent="@*android:style/Widget.Material.ProgressBar.Large">
        <item name="android:indeterminateOnly">true</item>
    </style>
    <style name="VersionTheme" parent="@*com.google.android.apps.docs:style/Theme.GoogleMaterial.Light.Bridge">
        <item name="android:statusBarColor">@*android:color/primary_text_leanback_light</item>
        <item name="*com.google.android.apps.docs:windowNoTitle">true</item>
        <item name="*com.google.android.apps.docs:colorPrimary">@*android:color/secondary_text_leanback_light</item>
        <item name="*com.google.android.apps.docs:colorPrimaryDark">@*android:color/primary_text_leanback_light</item>
        <item name="*com.google.android.apps.docs:colorAccent">@*com.google.android.apps.docs:color/google_blue500</item>
        <item name="*com.google.android.apps.docs:snackbarStyle">@*com.google.android.apps.docs:style/Widget.GoogleMaterial.Snackbar</item>
        <item name="*com.google.android.apps.docs:snackbarButtonStyle">@*com.google.android.apps.docs:style/Widget.GoogleMaterial.TextButton.Snackbar</item>
    </style>
</resources>
