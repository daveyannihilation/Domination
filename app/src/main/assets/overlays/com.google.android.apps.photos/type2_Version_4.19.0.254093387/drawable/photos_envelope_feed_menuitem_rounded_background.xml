<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <size android:height="@*com.google.android.apps.photos:dimen/photos_envelope_feed_menuitem_height" />
            <stroke android:width="1.0dip" android:color="@*android:color/white" />
            <corners android:radius="@*com.google.android.apps.photos:dimen/photos_envelope_feed_menuitem_radius" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <size android:height="@*com.google.android.apps.photos:dimen/photos_envelope_feed_menuitem_height" />
            <stroke android:width="1.0dip" android:color="@*android:color/white" />
            <solid android:color="@*android:color/dim_foreground_light" />
            <corners android:radius="@*com.google.android.apps.photos:dimen/photos_envelope_feed_menuitem_radius" />
        </shape>
    </item>
</selector>
