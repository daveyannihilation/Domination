<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ContextualToolbarTheme">
        <item name="android:colorBackground">@*android:color/primary_material_dark</item>
        <item name="*com.google.android.keep:colorControlNormal">@*com.google.android.keep:color/google_blue600</item>
    </style>
    <style name="DialogTheme" parent="@*com.google.android.keep:style/Theme.GoogleMaterial.DayNight.Dialog.MinWidth">
        <item name="android:windowBackground">@*android:color/background_floating_material_dark</item>
        <item name="android:windowTitleStyle">@*com.google.android.keep:style/TextAppearance.GoogleMaterial.Subhead1</item>
        <item name="android:buttonBarStyle">@*com.google.android.keep:style/DialogButtonBarStyle</item>
        <item name="android:buttonBarButtonStyle">@*com.google.android.keep:style/DialogFillButtonStyle</item>
        <item name="android:windowMinWidthMajor">65.0%</item>
        <item name="android:windowMinWidthMinor">95.00001%</item>
        <item name="android:buttonBarPositiveButtonStyle">@*com.google.android.keep:style/DialogFillButtonStyle</item>
        <item name="android:buttonBarNeutralButtonStyle">@*com.google.android.keep:style/DialogTextButtonStyle</item>
        <item name="android:buttonBarNegativeButtonStyle">@*com.google.android.keep:style/DialogTextButtonStyle</item>
        <item name="*com.google.android.keep:colorAccent">@*com.google.android.keep:color/button_color</item>
    </style>
    <style name="DividerStyle">
        <item name="android:theme">@*com.google.android.keep:style/Theme.GoogleMaterial.DayNight.Bridge</item>
        <item name="android:background">@*android:color/transparent</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.0dip</item>
        <item name="android:contentDescription">@null</item>
    </style>
    <style name="EditorConflictResolutionTimestampStyle">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#ffbbbbbb</item>
        <item name="android:paddingLeft">12.0dip</item>
        <item name="android:paddingTop">32.0dip</item>
        <item name="android:paddingRight">12.0dip</item>
    </style>
    <style name="LightToolbarStyle" parent="@*com.google.android.keep:style/Widget.GoogleMaterial.Toolbar">
        <item name="android:background">@*android:color/primary_material_dark</item>
        <item name="android:touchscreenBlocksFocus">true</item>
        <item name="*com.google.android.keep:titleTextAppearance">@*com.google.android.keep:style/Heading6TextAppearance</item>
    </style>
    <style name="ReminderDialogSpinner" parent="@*android:style/Widget.Material.Spinner">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="SearchBarDivider">
        <item name="android:background">@*android:color/transparent</item>
        <item name="android:layout_height">0.0dip</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>
    <style name="TextAppearance.Design.Snackbar.Message" parent="@*android:style/TextAppearance">
        <item name="android:textSize">@*com.google.android.keep:dimen/design_snackbar_text_size</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="ThemeOverlay.GoogleMaterial.Snackbar" parent="">
        <item name="*com.google.android.keep:colorOnSurface">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="ThemeOverlay.Popup" parent="@*com.google.android.keep:style/ThemeOverlay.AppCompat.Dark">
        <item name="android:background">@*android:color/background_dark</item>
        <item name="*com.google.android.keep:colorControlNormal">@*android:color/secondary_text_default_material_dark</item>
    </style>
    <style name="Widget.GoogleMaterial.Snackbar" parent="@*com.google.android.keep:style/Widget.MaterialComponents.Snackbar">
        <item name="*com.google.android.keep:actionTextColorAlpha">@*com.google.android.keep:dimen/design_snackbar_action_text_color_alpha</item>
        <item name="*com.google.android.keep:backgroundTint">@*android:color/background_floating_material_dark</item>
        <item name="*com.google.android.keep:enforceMaterialTheme">true</item>
        <item name="*com.google.android.keep:materialThemeOverlay">@*com.google.android.keep:style/ThemeOverlay.GoogleMaterial.Snackbar</item>
    </style>
    <style name="Widget.GoogleMaterial.Snackbar.TextView" parent="@*com.google.android.keep:style/Widget.MaterialComponents.Snackbar.TextView">
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="Widget.MaterialComponents.AppBarLayout.Surface" parent="@*com.google.android.keep:style/Widget.Design.AppBarLayout">
        <item name="android:background">@*android:color/primary_material_dark</item>
    </style>
</resources>
