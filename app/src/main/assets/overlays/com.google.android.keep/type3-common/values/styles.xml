<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ActionBarTitleFont" parent="@*com.google.android.keep:style/TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="Base.Theme.GoogleMaterial.Light" parent="@*com.google.android.keep:style/Base.V14.Theme.GoogleMaterial.Light">
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="ContextualToolbarTheme">
        <item name="android:colorBackground">@*android:color/background_floating_material_dark</item>
        <item name="*com.google.android.keep:colorControlNormal">@*com.google.android.keep:color/google_blue600</item>
        <item name="android:backgroundTint">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="EditorConflictAlertOverlayStyle">
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">#b3000000</item>
        <item name="android:padding">12.0dip</item>
    </style>
    <style name="EditorConflictAlertTextStyle">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">4.0dip</item>
    </style>
    <style name="EditorConflictResolutionTimestampStyle">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#ffbbbbbb</item>
        <item name="android:paddingLeft">12.0dip</item>
        <item name="android:paddingTop">32.0dip</item>
        <item name="android:paddingRight">12.0dip</item>
    </style>
    <style name="EmbeddedAccountMenuSideLayout">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:maxWidth">@*com.google.android.keep:dimen/og_nav_drawer_side_layout_width</item>
        <item name="android:minWidth">@*com.google.android.keep:dimen/og_nav_drawer_side_layout_width</item>
    </style>
    <style name="KeepPhotoViewerTheme" parent="@*com.google.android.keep:style/Theme.AppCompat">
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.keep:popupTheme">@*com.google.android.keep:style/ThemeOverlay.AppCompat</item>
    </style>
    <style name="LightToolbarStyle" parent="@*com.google.android.keep:style/Widget.GoogleMaterial.Toolbar">
        <item name="android:background">@*android:color/primary_material_dark</item>
        <item name="android:touchscreenBlocksFocus">true</item>
        <item name="*com.google.android.keep:titleTextAppearance">@*com.google.android.keep:style/Heading6TextAppearance</item>
    </style>
    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.DeviceDefault.Light" />
    <style name="RecipientEditTextViewBase">
        <item name="android:textAppearance">?*android:textAppearanceMedium</item>
        <item name="android:background">@null</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@*com.google.android.keep:dimen/chip_height</item>
        <item name="android:popupBackground">@*android:color/background_floating_material_dark</item>
        <item name="android:inputType">textEmailAddress|textMultiLine</item>
        <item name="android:textDirection">locale</item>
        <item name="android:textAlignment">viewStart</item>
    </style>
    <style name="ReminderDialogSpinner" parent="@*android:style/Widget.Holo.Spinner">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="SearchBarDivider">
        <item name="android:background">@*android:color/transparent</item>
        <item name="android:layout_height">0.0dip</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>
   <style name="ShareActivityInvalidShareeEmail">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:ellipsize">end</item>
        <item name="android:paddingTop">2.0dip</item>
        <item name="android:paddingBottom">2.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:fontFamily">sans-serif-light</item>
    </style>
    <style name="ShareActivityShareeEmail">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:paddingTop">2.0dip</item>
        <item name="android:paddingBottom">2.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:fontFamily">sans-serif-light</item>
    </style>
    <style name="TextAppearance.Design.Snackbar.Message" parent="@*android:style/TextAppearance">
        <item name="android:textSize">@*com.google.android.keep:dimen/design_snackbar_text_size</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="ThemeOverlay.GoogleMaterial.Light.BottomSheetDialog" parent="@*com.google.android.keep:style/ThemeOverlay.MaterialComponents.Dialog">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowAnimationStyle">@*com.google.android.keep:style/Animation.MaterialComponents.BottomSheetDialog</item>
        <item name="android:statusBarColor">@*android:color/transparent</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.keep:bottomSheetStyle">@*com.google.android.keep:style/Widget.GoogleMaterial.BottomSheet.Modal</item>
        <item name="*com.google.android.keep:materialButtonStyle">@*com.google.android.keep:style/Widget.GoogleMaterial.Button</item>
    </style>
    <style name="ThemeOverlay.GoogleMaterial.Snackbar" parent="">
        <item name="*com.google.android.keep:colorOnSurface">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="ToolbarTitleTextAppearance" parent="@*com.google.android.keep:style/TextAppearance.GoogleMaterial.Toolbar.Title">
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="Widget.GoogleMaterial.ProductLockupView.Dark" parent="@*com.google.android.keep:style/Widget.GoogleMaterial.ProductLockupView">
        <item name="*com.google.android.keep:productNameTextColor">@*android:color/white</item>
        <item name="*com.google.android.keep:logoColor">light</item>
    </style>
    <style name="WidgetHeaderTitleStyle">
        <item name="android:textAppearance">@*com.google.android.keep:style/Heading6TextAppearance</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:layout_gravity">center|left</item>
        <item name="android:paddingLeft">8.0dip</item>
        <item name="android:paddingTop">6.0dip</item>
        <item name="android:paddingBottom">6.0dip</item>
    </style>
    <style name="ZoomingActivityThemeLight" parent="@*android:style/Theme.Holo">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowActionBar">true</item>
    </style>
</resources>
