<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BottomSheetDialogTheme" parent="@*com.google.android.apps.magazines:style/ThemeOverlay.MaterialComponents.BottomSheetDialog">
        <item name="android:windowIsFloating">false</item>
        <item name="android:statusBarColor">@*android:color/transparent</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@*android:color/transparent</item>
        <item name="*com.google.android.apps.magazines:bottomSheetStyle">@*com.google.android.apps.magazines:style/BottomSheet</item>
    </style>
    <style name="NSTheme.DayNight" parent="@*com.google.android.apps.magazines:style/NSTheme.Night">
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@*android:color/primary_dark_material_dark</item>
    </style>
</resources>
