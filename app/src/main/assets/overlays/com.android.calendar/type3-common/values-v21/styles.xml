<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CalendarTheme.WithActionBar" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:actionBarStyle">@*com.android.calendar:style/CalendarActionBarStyle</item>
        <item name="android:endYear">2036</item>
        <item name="android:startYear">1970</item>
        <item name="android:colorPrimary">#ff303f9f</item>
        <item name="android:colorPrimaryDark">#ff1a237e</item>
        <item name="android:colorAccent">#ff1976d2</item>
        <item name="android:windowActionBar">true</item>
        <item name="android:windowNoTitle">false</item>
    </style>
    <style name="Settings" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:colorPrimary">#ff303f9f</item>
        <item name="android:colorPrimaryDark">#ff1a237e</item>
        <item name="android:colorAccent">#ff1976d2</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
    </style>
    <style name="Sync" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:colorPrimary">#ff303f9f</item>
        <item name="android:colorPrimaryDark">#ff1a237e</item>
        <item name="android:colorAccent">#ff1976d2</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
    </style>
</resources>
