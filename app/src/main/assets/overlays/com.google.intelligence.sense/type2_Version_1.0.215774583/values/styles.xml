<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AmbientMusicSettingsTheme" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:colorAccent">@*com.google.intelligence.sense:color/quantum_googblue</item>
        <item name="*com.google.intelligence.sense:preferenceTheme">@*com.google.intelligence.sense:style/AmbientMusicSettingsPreferenceTheme</item>
    </style>
    <style name="DndSettingsTheme" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:listPreferredItemHeight">72.0dip</item>
        <item name="android:listPreferredItemPaddingStart">@*com.google.intelligence.sense:dimen/settings_start_margin</item>
        <item name="*com.google.intelligence.sense:switchBarTheme">@*com.google.intelligence.sense:style/ThemeOverlay.SwitchBar.SubSettings</item>
        <item name="*com.google.intelligence.sense:preferenceTheme">@*com.google.intelligence.sense:style/PreferenceThemeOverlay.v14.Material</item>
    </style>
    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.DeviceDefault.Light" />
    <style name="SongActionsBottomSheetDialogTheme" parent="@*com.google.intelligence.sense:style/Theme.Design.Light.BottomSheetDialog">
        <item name="android:windowIsFloating">false</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.intelligence.sense:bottomSheetStyle">@*com.google.intelligence.sense:style/BottomSheetStyle</item>
    </style>
    <style name="SuwGlifCardContainer">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:elevation">@*com.google.intelligence.sense:dimen/suw_glif_card_elevation</item>
    </style>
    <style name="TrackSubtitle">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:lineSpacingExtra">0.0sp</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="TrackTitle">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:singleLine">true</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="Widget.Design.BottomSheet.Modal" parent="@*android:style/Widget">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:elevation">@*com.google.intelligence.sense:dimen/design_bottom_sheet_modal_elevation</item>
        <item name="*com.google.intelligence.sense:behavior_peekHeight">auto</item>
        <item name="*com.google.intelligence.sense:behavior_hideable">true</item>
        <item name="*com.google.intelligence.sense:behavior_skipCollapsed">false</item>
    </style>
</resources>
