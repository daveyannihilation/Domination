<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Platform.V25.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar" />
    <style name="Widget.Design.BottomSheet.Modal" parent="@*android:style/Widget">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:elevation">@*com.google.android.soundpicker:dimen/design_bottom_sheet_modal_elevation</item>
        <item name="*com.google.android.soundpicker:behavior_peekHeight">auto</item>
        <item name="*com.google.android.soundpicker:behavior_hideable">true</item>
        <item name="*com.google.android.soundpicker:behavior_skipCollapsed">false</item>
        <item name="*com.google.android.soundpicker:shapeAppearance">@null</item>
        <item name="*com.google.android.soundpicker:shapeAppearanceOverlay">@null</item>
        <item name="*com.google.android.soundpicker:enforceMaterialTheme">false</item>
        <item name="*com.google.android.soundpicker:backgroundTint">@*android:color/background_floating_material_dark</item>
    </style>
</resources>
