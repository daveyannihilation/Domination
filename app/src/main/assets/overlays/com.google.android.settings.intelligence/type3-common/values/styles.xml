<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Base.V27.Theme.GoogleMaterial.Light" parent="@*com.google.android.settings.intelligence:style/Base.V24.Theme.GoogleMaterial.Dark">
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>
    <style name="Base.V27.Theme.GoogleMaterial.Light.BottomSheetDialog" parent="@*com.google.android.settings.intelligence:style/Base.V21.Theme.GoogleMaterial.Dark.BottomSheetDialog">
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@*android:color/transparent</item>
    </style>
    <style name="Theme.PlacePickerConfirmation" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:textColorHint">@*android:color/secondary_text_default_material_dark</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:navigationBarColor">@*android:color/transparent</item>
    </style>
    <style name="Theme.Settings" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:textColorHint">@*android:color/secondary_text_default_material_dark</item>
        <item name="android:navigationBarColor">@*android:color/transparent</item>
    </style>
</resources>
