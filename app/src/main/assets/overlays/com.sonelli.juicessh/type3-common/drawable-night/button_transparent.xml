<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <solid android:color="#80000000" />
            <stroke android:width="3.0px" android:color="@*android:color/white" />
            <padding android:left="10.0dip" android:top="4.0dip" android:right="10.0dip" android:bottom="4.0dip" />
            <corners android:topLeftRadius="5.0dip" android:topRightRadius="5.0dip" android:bottomLeftRadius="5.0dip" android:bottomRightRadius="5.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#80555555" />
            <stroke android:width="3.0px" android:color="#33555555" />
            <padding android:left="10.0dip" android:top="4.0dip" android:right="10.0dip" android:bottom="4.0dip" />
            <corners android:topLeftRadius="5.0dip" android:topRightRadius="5.0dip" android:bottomLeftRadius="5.0dip" android:bottomRightRadius="5.0dip" />
        </shape>
    </item>
</selector>
