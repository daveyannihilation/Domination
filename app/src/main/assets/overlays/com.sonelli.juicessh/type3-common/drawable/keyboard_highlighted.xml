<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ff555555" />
            <stroke android:width="1.0px" android:color="#ff999999" />
            <padding android:left="2.0dip" android:top="10.0dip" android:right="2.0dip" android:bottom="10.0dip" />
            <corners android:topLeftRadius="4.0dip" android:topRightRadius="4.0dip" android:bottomLeftRadius="4.0dip" android:bottomRightRadius="4.0dip" />
            <gradient android:startColor="#ff555555" android:endColor="#ff555555" android:angle="90.0" android:type="linear" android:centerColor="#ff333333" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <solid android:color="#ff555555" />
            <stroke android:width="1.0px" android:color="#ff999999" />
            <padding android:left="2.0dip" android:top="10.0dip" android:right="2.0dip" android:bottom="10.0dip" />
            <corners android:topLeftRadius="4.0dip" android:topRightRadius="4.0dip" android:bottomLeftRadius="4.0dip" android:bottomRightRadius="4.0dip" />
            <gradient android:startColor="#ff555555" android:endColor="#ff555555" android:angle="90.0" android:type="linear" android:centerColor="#ff333333" />
        </shape>
    </item>
</selector>