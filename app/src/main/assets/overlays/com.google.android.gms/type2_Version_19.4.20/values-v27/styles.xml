        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>
    <style name="Theme.Ocr.AppCompat.Dark" parent="@*com.google.android.gms:style/Base.Theme.Ocr.AppCompat.Dark">
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>
    <style name="TpActivityTheme.NoActionBar" parent="@*com.google.android.gms:style/TpActivityTheme">
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="*com.google.android.gms:windowActionBar">false</item>
        <item name="*com.google.android.gms:windowNoTitle">true</item>
    </style>
</resources>
