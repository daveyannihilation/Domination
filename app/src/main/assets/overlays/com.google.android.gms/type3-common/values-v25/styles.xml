<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Platform.V25.AppCompat" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:backgroundTint">@*android:color/background_dark</item>
    </style>
    <style name="Platform.V25.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:backgroundTint">@*android:color/background_dark</item>
    </style>
    <style name="common.Theme.Base.NoAppCompat.Light.Dialog" parent="@*android:style/Theme.Material.Dialog">
        <item name="android:colorAccent">@*com.google.android.gms:color/material_google_blue_500</item>
        <item name="*com.google.android.gms:buttonBarStyle">?android:buttonBarStyle</item>
        <item name="*com.google.android.gms:buttonBarButtonStyle">?android:buttonBarButtonStyle</item>
        <item name="*com.google.android.gms:dividerHorizontal">@null</item>
        <item name="android:background">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="common.Theme.Base.NoAppCompat.Light.Dialog.MinWidth" parent="@*android:style/Theme.Material.Dialog.MinWidth">
        <item name="android:colorAccent">@*com.google.android.gms:color/material_google_blue_500</item>
        <item name="*com.google.android.gms:buttonBarStyle">?android:buttonBarStyle</item>
        <item name="*com.google.android.gms:buttonBarButtonStyle">?android:buttonBarButtonStyle</item>
        <item name="*com.google.android.gms:dividerHorizontal">@null</item>
        <item name="android:background">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="TrustAgentNotificationWithNoActionBar" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:windowBackground">@*android:color/background_dark</item>
    </style>



    <style name="completeDialogTheme" parent="@*android:style/Theme.Material.Dialog" />
</resources>
