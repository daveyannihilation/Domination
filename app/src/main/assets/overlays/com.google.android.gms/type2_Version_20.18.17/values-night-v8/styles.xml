<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Sharing.CardActivity" parent="@*com.google.android.gms:style/Theme.GoogleMaterial.Dark.NoActionBar.Bridge">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:homeAsUpIndicator">@*com.google.android.gms:drawable/quantum_gm_ic_clear_vd_theme_24</item>
        <item name="android:fontFamily">@*com.google.android.gms:font/google_sans_medium</item>
        <item name="android:windowContentTransitions">true</item>
        <item name="android:strokeColor">@*android:color/white</item>
        <item name="android:colorButtonNormal">@*com.google.android.gms:color/sharing_color_accent</item>
        <item name="android:windowEnterTransition">@*com.google.android.gms:transition/sharing_window_enter_transition</item>
        <item name="android:windowSharedElementEnterTransition">@*com.google.android.gms:transition/sharing_window_shared_element_enter_transition</item>
        <item name="android:windowSharedElementExitTransition">@*com.google.android.gms:transition/sharing_window_shared_element_exit_transition</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@*android:color/transparent</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowSharedElementReturnTransition">@*com.google.android.gms:transition/sharing_window_shared_element_return_transition</item>
        <item name="android:windowSharedElementReenterTransition">@*com.google.android.gms:transition/sharing_window_shared_element_reenter_transition</item>
        <item name="android:windowSharedElementsUseOverlay">true</item>
        <item name="android:windowActivityTransitions">true</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.gms:colorAccent">@*com.google.android.gms:color/sharing_color_accent</item>
        <item name="*com.google.android.gms:colorButtonNormal">@*com.google.android.gms:color/sharing_color_accent</item>
        <item name="*com.google.android.gms:colorError">@*com.google.android.gms:color/sharing_color_error</item>
        <item name="*com.google.android.gms:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.google.android.gms:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>
</resources>
