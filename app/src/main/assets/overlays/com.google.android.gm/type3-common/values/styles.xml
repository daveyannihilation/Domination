<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AdBadgeBaseTextStyle">
        <item name="android:textSize">@*com.google.android.gm:dimen/conversation_sender_header_font_size</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="BadgeTextStyle">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="CalendarPromotionStyle">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:background">@*android:color/background_dark</item>
        <item name="android:paddingBottom">20.0dip</item>
        <item name="android:layout_margin">@*com.google.android.gm:dimen/calendar_promotion_layout_margin</item>
        <item name="android:minHeight">60.0dip</item>
        <item name="android:paddingEnd">20.0dip</item>
    </style>
    <style name="ComposeFieldLayout">
        <item name="android:orientation">horizontal</item>
        <item name="android:focusable">false</item>
        <item name="android:focusableInTouchMode">false</item>
        <item name="android:addStatesFromChildren">true</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:background">#ff777777</item>
        <item name="android:shadowColor">@*android:color/white</item>
        <item name="android:shadowDx">1.5</item>
        <item name="android:shadowDy">1.3</item>
        <item name="android:shadowRadius">8.0</item>
    </style>
    <style name="ConversationListFadeBase" parent="@*android:style/Widget.Holo.ListView" />
    <style name="GmailGoogleMaterialActionBar" parent="@*com.google.android.gm:style/ThemeOverlay.GoogleMaterial.ActionBar">
        <item name="android:textColorPrimary">@*com.google.android.gm:color/action_bar_icon_enabled</item>
        <item name="android:textColor">@*com.google.android.gm:color/ag_primary_text</item>
        <item name="android:background">@*android:color/transparent</item>
        <item name="*com.google.android.gm:colorAccent">@*android:color/white</item>
        <item name="*com.google.android.gm:colorControlNormal">@*com.google.android.gm:color/ag_secondary_text</item>
        <item name="*com.google.android.gm:titleTextColor">@*com.google.android.gm:color/primary_color_red</item>
    </style>
    <style name="GmailGoogleMaterialDatePickerDialogTheme" parent="@*com.google.android.gm:style/ThemeOverlay.GoogleMaterial.Dialog">
        <item name="android:textColorPrimary">@*com.google.android.gm:color/ag_primary_text</item>
        <item name="android:textColorPrimaryInverse">@*android:color/white</item>
        <item name="android:windowBackground">@*com.google.android.gm:drawable/dialog_background</item>
        <item name="android:buttonBarButtonStyle">@*com.google.android.gm:style/GmailGoogleMaterialButtonBarStyle</item>
        <item name="android:datePickerStyle">@*com.google.android.gm:style/DatePickerStyle</item>
    </style>
    <style name="GmailTheme.NoActionBar" parent="@*android:style/Theme.Material.NoActionBar" />
    <style name="GmailifyTheme" parent="@*com.google.android.gm:style/GmailifyTheme.DarkThemeSupport">
        <item name="android:spinnerStyle">@*android:style/Widget.Holo.Spinner</item>
        <item name="android:textColorLink">@*com.google.android.gm:color/text_color_blue</item>
        <item name="android:actionModeBackground">@*com.google.android.gm:color/action_mode_background</item>
        <item name="android:actionModeCloseDrawable">@*com.google.android.gm:drawable/ic_check_wht_24dp</item>
    </style>
    <style name="HatsText.PromoButton.Action" parent="@*com.google.android.gm:style/HatsText.PromoButton">
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:background">@*com.google.android.gm:drawable/primary_action_button_background</item>
    </style>
    <style name="MailActivityTheme" parent="@*com.google.android.gm:style/BaseMailActivityTheme">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowTitleStyle">@*com.google.android.gm:style/GmailDialogTitleTextAppearance</item>
        <item name="android:alertDialogTheme">@*com.google.android.gm:style/AlertDialogTheme</item>
        <item name="android:borderlessButtonStyle">@*com.google.android.gm:style/GmailGoogleMaterialBorderlessButtonStyle</item>
        <item name="*com.google.android.gm:drawerArrowStyle">@*com.google.android.gm:style/Widget.AppCompat.DrawerArrowToggle</item>
        <item name="*com.google.android.gm:windowActionModeOverlay">true</item>
        <item name="*com.google.android.gm:actionModeStyle">@*com.google.android.gm:style/GmailActionModeStyle</item>
        <item name="*com.google.android.gm:popupMenuStyle">@*com.google.android.gm:style/MessageHeaderPopupStyle</item>
        <item name="*com.google.android.gm:colorPrimary">@*com.google.android.gm:color/primary_color</item>
        <item name="*com.google.android.gm:colorPrimaryDark">@*com.google.android.gm:color/primary_dark_color</item>
        <item name="*com.google.android.gm:colorControlNormal">@*android:color/secondary_text_default_material_dark</item>
        <item name="*com.google.android.gm:alertDialogTheme">@*com.google.android.gm:style/AlertDialogTheme</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="Promo.Text" parent="@*com.google.android.gm:style/Promo">
        <item name="android:textSize">@*com.google.android.gm:dimen/promo_body_text_size</item>
        <item name="android:textColor">@*com.google.android.gm:color/primary_color</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textAlignment">viewStart</item>
        <item name="android:layout_marginStart">@*com.google.android.gm:dimen/promo_inner_padding</item>
        <item name="android:layout_marginEnd">@*com.google.android.gm:dimen/promo_inner_padding</item>
    </style>
    <style name="PromoBadge">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">end|center</item>
        <item name="android:paddingLeft">@*com.google.android.gm:dimen/promo_badge_padding_left</item>
        <item name="android:paddingTop">@*com.google.android.gm:dimen/promo_badge_padding_top</item>
        <item name="android:paddingRight">@*com.google.android.gm:dimen/promo_badge_padding_right</item>
        <item name="android:paddingBottom">@*com.google.android.gm:dimen/promo_badge_padding_bottom</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">3.0dip</item>
        <item name="android:layout_marginBottom">3.0dip</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="RecipientComposeFieldLayout" parent="@*com.google.android.gm:style/ComposeFieldLayout">
        <item name="android:orientation">vertical</item>
        <item name="android:paddingTop">0.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:background">@*android:color/background_dark</item>
    </style>
    <style name="RecipientEditTextViewBase" parent="@*android:attr/autoCompleteTextViewStyle">
        <item name="android:textAppearance">?*android:textAppearanceMedium</item>
        <item name="android:background">@null</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@*com.google.android.gm:dimen/chip_height</item>
        <item name="android:popupBackground">@*android:color/background_floating_material_dark</item>
        <item name="android:inputType">textEmailAddress|textMultiLine</item>
        <item name="android:textDirection">locale</item>
        <item name="android:textAlignment">viewStart</item>
    </style>
    <style name="SudGlifCardContainer">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="ToRecipientEditTextViewStyle" parent="@*com.google.android.gm:style/RecipientEditTextViewStyle">
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:shadowColor">@*android:color/white</item>
        <item name="android:shadowDx">1.5</item>
        <item name="android:shadowDy">1.3</item>
        <item name="android:shadowRadius">8.0</item>
    </style>
    <style name="UnseenCountBase">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">end|center</item>
        <item name="android:paddingTop">@*com.google.android.gm:dimen/drawer_folder_teaser_pills_padding</item>
        <item name="android:paddingBottom">@*com.google.android.gm:dimen/drawer_folder_teaser_pills_padding</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">3.0dip</item>
        <item name="android:layout_marginBottom">3.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:paddingStart">6.0dip</item>
        <item name="android:paddingEnd">6.0dip</item>
    </style>
    <style name="VacationResponderActionButtonText">
        <item name="android:textAppearance">?*android:actionMenuTextAppearance</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">none</item>
        <item name="android:padding">4.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="Widget.GoogleMaterial.ProductLockupView.Dark" parent="@*com.google.android.gm:style/Widget.GoogleMaterial.ProductLockupView">
        <item name="*com.google.android.gm:logoColor">light</item>
        <item name="*com.google.android.gm:productNameTextColor">@*android:color/white</item>
    </style>
</resources>
