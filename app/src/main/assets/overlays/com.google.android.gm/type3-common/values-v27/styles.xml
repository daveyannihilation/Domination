<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="DarkNavigationBarBottomSheetTheme" parent="@*com.google.android.gm:style/ThemeOverlay.MaterialComponents.Dialog">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowAnimationStyle">@*com.google.android.gm:style/Animation.Design.BottomSheetDialog</item>
        <item name="android:statusBarColor">@*android:color/transparent</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="*com.google.android.gm:bottomSheetStyle">@*com.google.android.gm:style/Widget.Design.BottomSheet.Modal</item>
    </style>
    <style name="SudThemeGlifV3.Light" parent="@*com.google.android.gm:style/SudBaseThemeGlifV3.Light">
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.gm:sucLightSystemNavBar">false</item>
        <item name="*com.google.android.gm:sucSystemNavBarDividerColor">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="Base.V27.Theme.GoogleMaterial.Light" parent="@*com.google.android.gm:style/Base.V24.Theme.GoogleMaterial.Dark">
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>
    <style name="Base.V27.Theme.GoogleMaterial.Light.BottomSheetDialog" parent="@*com.google.android.gm:style/Base.V21.Theme.GoogleMaterial.Dark.BottomSheetDialog">
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@*android:color/transparent</item>
    </style>
</resources>
