<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ColoredNavBarGeneralTheme" parent="@*com.google.android.gm:style/BaseGeneralTheme">
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="ComposeThemeBase" parent="@*com.google.android.gm:style/ColoredNavBarGeneralTheme">
        <item name="android:colorBackground">@*android:color/background_dark</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:dropDownListViewStyle">@*com.google.android.gm:style/ComposeDropdownListViewStyle</item>
        <item name="android:colorBackgroundCacheHint">@*android:color/background_dark</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="*com.google.android.gm:popupMenuStyle">@*com.google.android.gm:style/PopupMenu</item>
    </style>
    <style name="MailActivityTheme" parent="@*com.google.android.gm:style/BaseMailActivityTheme">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowTitleStyle">@*com.google.android.gm:style/GmailDialogTitleTextAppearance</item>
        <item name="android:alertDialogTheme">@*com.google.android.gm:style/AlertDialogTheme</item>
        <item name="android:borderlessButtonStyle">@*com.google.android.gm:style/GmailGoogleMaterialBorderlessButtonStyle</item>
        <item name="android:colorAccent">@*com.google.android.gm:color/color_accent_blue</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@*com.google.android.gm:color/mail_activity_status_bar_color</item>
        <item name="android:navigationBarColor">@*com.google.android.gm:color/navigation_bar_color</item>
        <item name="android:timePickerDialogTheme">@*com.google.android.gm:style/GmailGoogleMaterialTimePickerDialogTheme</item>
        <item name="android:datePickerDialogTheme">@*com.google.android.gm:style/GmailGoogleMaterialDatePickerDialogTheme</item>
        <item name="android:colorEdgeEffect">@*com.google.android.gm:color/edge_effect_color</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@*com.google.android.gm:color/navigation_bar_divider_color</item>
        <item name="*com.google.android.gm:drawerArrowStyle">@*com.google.android.gm:style/Widget.AppCompat.DrawerArrowToggle</item>
        <item name="*com.google.android.gm:windowActionModeOverlay">true</item>
        <item name="*com.google.android.gm:actionModeStyle">@*com.google.android.gm:style/GmailActionModeStyle</item>
        <item name="*com.google.android.gm:popupMenuStyle">@*com.google.android.gm:style/MessageHeaderPopupStyle</item>
        <item name="*com.google.android.gm:colorPrimary">@*com.google.android.gm:color/primary_color</item>
        <item name="*com.google.android.gm:colorPrimaryDark">@*com.google.android.gm:color/primary_dark_color</item>
        <item name="*com.google.android.gm:colorControlNormal">@*com.google.android.gm:color/default_icon_color</item>
        <item name="*com.google.android.gm:alertDialogTheme">@*com.google.android.gm:style/AlertDialogTheme</item>
    </style>
    <style name="SudThemeGlifV2.Light" parent="@*com.google.android.gm:style/SudThemeGlif.Light">
        <item name="android:colorBackground">@*com.google.android.gm:color/sud_glif_background_color_light</item>
        <item name="android:editTextStyle">@*com.google.android.gm:style/SudEditText</item>
        <item name="android:alertDialogTheme">@*com.google.android.gm:style/SudAlertDialogTheme.Light</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="*com.google.android.gm:sudDividerInsetEnd">@*com.google.android.gm:dimen/sud_glif_margin_sides</item>
        <item name="*com.google.android.gm:sudDividerInsetStart">@*com.google.android.gm:dimen/sud_glif_margin_sides</item>
        <item name="*com.google.android.gm:sudDividerInsetStartNoIcon">@*com.google.android.gm:dimen/sud_glif_margin_sides</item>
        <item name="*com.google.android.gm:sudEditTextBackgroundColor">@*com.google.android.gm:color/sud_glif_edit_text_bg_light_color</item>
        <item name="*com.google.android.gm:sudGlifHeaderGravity">center_horizontal</item>
        <item name="*com.google.android.gm:sudScrollIndicators">bottom|top</item>
        <item name="*com.google.android.gm:sucLightStatusBar">false</item>
        <item name="*com.google.android.gm:sudBackgroundPatterned">false</item>
        <item name="*com.google.android.gm:sudBackgroundBaseColor">@*android:color/background_floating_material_dark</item>
    </style>
</resources>
