    <style name="Base.Widget.AppCompat.Light.ActionBar.TabView" parent="@*android:style/Widget.Material.ActionBar.TabView" />
    <style name="Base.Widget.AppCompat.Light.PopupMenu" parent="@*android:style/Widget.Material.PopupMenu" />
    <style name="ComposeThemeBase" parent="@*com.google.android.gm:style/ColoredNavBarGeneralTheme">
        <item name="android:colorBackground">@*android:color/background_dark</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:dropDownListViewStyle">@*com.google.android.gm:style/ComposeDropdownListViewStyle</item>
        <item name="android:colorBackgroundCacheHint">@*android:color/background_dark</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="*com.google.android.gm:popupMenuStyle">@*com.google.android.gm:style/PopupMenu</item>
    </style>
    <style name="ConversationListFadeBase" parent="@*android:style/Widget.Material.ListView" />
    <style name="GmailifyTheme" parent="@*com.google.android.gm:style/GmailifyTheme.DarkThemeSupport">
        <item name="android:spinnerStyle">@*android:style/Widget.Material.Spinner</item>
        <item name="android:textColorLink">@*com.google.android.gm:color/text_color_blue</item>
        <item name="android:colorPrimaryDark">@*com.google.android.gm:color/ag_blue900</item>
    </style>
    <style name="LabelSyncTheme" parent="@*com.google.android.gm:style/LabelSyncTheme.DarkThemeSupport">
        <item name="android:textColorPrimary">@*com.google.android.gm:color/ag_primary_text</item>
        <item name="android:background">@*android:color/background_dark</item>
        <item name="android:colorControlNormal">@*com.google.android.gm:color/ag_accent_blue</item>
        <item name="android:colorControlActivated">@*com.google.android.gm:color/ag_accent_blue</item>
        <item name="android:colorAccent">@*com.google.android.gm:color/ag_accent_blue</item>
    </style>
    <style name="Platform.V21.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorHintInverse">#ffbbbbbb</item>
        <item name="android:textColorHint">#ffbbbbbb</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
        <item name="android:textColorLinkInverse">@*android:color/accent_material_dark</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>
    <style name="SudGlifCardContainer">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:elevation">@*com.google.android.gm:dimen/sud_glif_card_elevation</item>
    </style>
    <style name="SudThemeGlif" parent="@*com.google.android.gm:style/SudBaseThemeGlif">
        <item name="*com.google.android.gm:sucSystemNavBarBackgroundColor">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="SudThemeGlif.Light" parent="@*com.google.android.gm:style/SudBaseThemeGlif">
        <item name="*com.google.android.gm:sucSystemNavBarBackgroundColor">@*android:color/primary_dark_material_dark</item>
    </style>
