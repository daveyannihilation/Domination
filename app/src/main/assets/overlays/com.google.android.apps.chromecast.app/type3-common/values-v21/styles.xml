<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme.NonTransparentStatusBar" parent="@*com.google.android.apps.chromecast.app:style/AppTheme">
        <item name="android:windowTranslucentStatus">false</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.apps.chromecast.app:windowActionBar">false</item>
        <item name="*com.google.android.apps.chromecast.app:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="Base.TextAppearance.AppCompat.Inverse" parent="@*android:style/TextAppearance.Material" />
    <style name="Base.TextAppearance.AppCompat.Large.Inverse" parent="@*android:style/TextAppearance.Material.Large" />
    <style name="Base.TextAppearance.AppCompat.Medium.Inverse" parent="@*android:style/TextAppearance.Material.Medium" />
    <style name="Base.TextAppearance.AppCompat.Small.Inverse" parent="@*android:style/TextAppearance.Material.Small" />
    <style name="Base.TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse" parent="@*android:style/TextAppearance.Material.Widget.ActionBar.Title" />
    <style name="Base.Theme.GoogleMaterial.Light" parent="@*com.google.android.apps.chromecast.app:style/Base.V14.Theme.GoogleMaterial.Light">
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabText" parent="@*android:style/Widget.Material.ActionBar.TabText" />
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabView" parent="@*android:style/Widget.Material.ActionBar.TabView" />
    <style name="Base.Widget.AppCompat.Light.PopupMenu" parent="@*android:style/Widget.Material.PopupMenu" />
    <style name="Button.Flat.Gray" parent="@*com.google.android.apps.chromecast.app:style/Button.Flat">
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:alpha">@*com.google.android.apps.chromecast.app:dimen/low_opacity</item>
    </style>
    <style name="MediaCardDevicePickerSection" parent="@*com.google.android.apps.chromecast.app:style/RegularFontFamily.Medium">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:letterSpacing">0.125</item>
    </style>
    <style name="MediaCardProviderLabel" parent="@*com.google.android.apps.chromecast.app:style/RegularFontFamily.Medium">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:letterSpacing">0.16</item>
    </style>
    <style name="MediaCardSubtitle" parent="@*com.google.android.apps.chromecast.app:style/RegularFontFamily.Small">
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">1</item>
        <item name="android:letterSpacing">0.02</item>
        <item name="android:fontFamily">@*com.google.android.apps.chromecast.app:font/google_sans_medium</item>
    </style>
    <style name="MediaCardTitle" parent="@*com.google.android.apps.chromecast.app:style/RegularFontFamily.Medium">
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">1</item>
        <item name="android:letterSpacing">0.02</item>
        <item name="android:fontFamily">@*com.google.android.apps.chromecast.app:font/google_sans_medium</item>
    </style>
    <style name="SetupTitleText">
        <item name="android:textSize">25.0sp</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:fontFamily">@*com.google.android.apps.chromecast.app:font/google_sans</item>
    </style>
    <style name="SuwGlifCardBackground">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="SuwGlifCardContainer">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:elevation">@*com.google.android.apps.chromecast.app:dimen/suw_glif_card_elevation</item>
    </style>
    <style name="Widget.Design.BottomSheet.Modal" parent="@*android:style/Widget">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:elevation">@*com.google.android.apps.chromecast.app:dimen/design_bottom_sheet_modal_elevation</item>
        <item name="*com.google.android.apps.chromecast.app:behavior_peekHeight">auto</item>
        <item name="*com.google.android.apps.chromecast.app:behavior_hideable">true</item>
        <item name="*com.google.android.apps.chromecast.app:behavior_skipCollapsed">false</item>
        <item name="*com.google.android.apps.chromecast.app:shapeAppearance">@null</item>
        <item name="*com.google.android.apps.chromecast.app:shapeAppearanceOverlay">@null</item>
        <item name="*com.google.android.apps.chromecast.app:enforceMaterialTheme">false</item>
        <item name="*com.google.android.apps.chromecast.app:backgroundTint">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="Platform.V21.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorHintInverse">#ffbbbbbb</item>
        <item name="android:textColorHint">#ffbbbbbb</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
        <item name="android:textColorLinkInverse">@*android:color/accent_material_dark</item>
    </style>
</resources>
