<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme.NonTransparentStatusBar.Light" parent="@*com.google.android.apps.chromecast.app:style/AppThemeDark">
        <item name="android:windowTranslucentStatus">false</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.apps.chromecast.app:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.apps.chromecast.app:windowActionBar">false</item>
    </style>
    <style name="AppTheme.SolidStatusBar" parent="@*com.google.android.apps.chromecast.app:style/AppTheme.SolidStatusBarBase">
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.apps.chromecast.app:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.apps.chromecast.app:windowActionBar">false</item>
    </style>
    <style name="AppTheme.SolidStatusBarBase" parent="@*com.google.android.apps.chromecast.app:style/AppTheme">
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="CardView" parent="@*com.google.android.apps.chromecast.app:style/Base.CardView">
        <item name="*com.google.android.apps.chromecast.app:cardBackgroundColor">@*android:color/primary_material_dark</item>
    </style>
    <style name="Base.V23.Theme.GoogleMaterial.Light" parent="@*com.google.android.apps.chromecast.app:style/Base.V21.Theme.GoogleMaterial.Dark">
        <item name="android:windowLightStatusBar">false</item>
    </style>
</resources>
