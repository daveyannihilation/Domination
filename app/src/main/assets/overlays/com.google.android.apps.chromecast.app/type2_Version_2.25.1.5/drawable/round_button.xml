<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <selector>
            <item android:state_pressed="false">
                <shape android:shape="oval">
                    <solid android:color="@*android:color/button_material_dark" />
                </shape>
            </item>
        </selector>
    </item>
    <item>
        <selector>
            <item android:state_pressed="true">
                <shape android:shape="oval">
                    <solid android:color="@*android:color/dim_foreground_light" />
                </shape>
            </item>
        </selector>
    </item>
</layer-list>
