<?xml version="1.0" encoding="utf-8"?>
<selector android:shape="rectangle"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="10.0dip" />
            <solid android:color="@*android:color/dim_foreground_light" />
            <stroke android:width="3.0dip" android:color="@*android:color/accent_material_dark" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="10.0dip" />
            <solid android:color="@*android:color/button_material_dark" />
            <stroke android:width="3.0dip" android:color="@*android:color/accent_material_dark" />
        </shape>
    </item>
</selector>
