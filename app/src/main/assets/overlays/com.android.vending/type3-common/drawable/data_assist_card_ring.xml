<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <shape android:shape="ring" android:innerRadiusRatio="2.1" android:thicknessRatio="48.0" android:useLevel="false">
            <solid android:color="@*com.android.vending:color/myapps_progress_ring_bg" />
        </shape>
    </item>
    <item android:id="@*com.android.vending:id/progress">
        <rotate android:fromDegrees="-90.0" android:toDegrees="-90.0">
            <shape android:shape="ring" android:innerRadiusRatio="2.1" android:thicknessRatio="48.0" android:useLevel="true">
                <solid android:color="@*android:color/white" />
            </shape>
        </rotate>
    </item>
</layer-list>
