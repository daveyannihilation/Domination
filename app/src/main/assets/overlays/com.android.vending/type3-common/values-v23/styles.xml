        <item name="*com.android.vending:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>

    <style name="SuwThemeGlifV2.Light" parent="@*com.android.vending:style/SuwThemeGlif">
        <item name="android:colorBackground">@*android:color/background_floating_material_dark</item>
        <item name="android:editTextStyle">@*com.android.vending:style/SuwEditText</item>
        <item name="android:alertDialogTheme">@*com.android.vending:style/SuwAlertDialogTheme</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="*com.android.vending:suwBackgroundBaseColor">@*android:color/background_dark</item>
        <item name="*com.android.vending:suwBackgroundPatterned">false</item>
        <item name="*com.android.vending:suwDividerInsetEnd">@*com.android.vending:dimen/suw_layout_margin_sides</item>
        <item name="*com.android.vending:suwDividerInsetStart">@*com.android.vending:dimen/suw_layout_margin_sides</item>
        <item name="*com.android.vending:suwDividerInsetStartNoIcon">@*com.android.vending:dimen/suw_layout_margin_sides</item>
        <item name="*com.android.vending:suwEditTextBackgroundColor">@*android:color/background_floating_material_dark</item>
        <item name="*com.android.vending:suwGlifHeaderGravity">center_horizontal</item>
        <item name="*com.android.vending:suwScrollIndicators">bottom|top</item>
