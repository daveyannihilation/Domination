<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CardView" parent="@*com.android.vending:style/Base.CardView">
        <item name="*com.android.vending:cardBackgroundColor">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="LeftNavRedesignWhiteTheme" parent="@*com.android.vending:style/Theme.AppCompat">
        <item name="android:textColorPrimary">@*com.android.vending:color/play_primary_settings_set</item>
        <item name="android:textAppearanceMedium">@*com.android.vending:style/DarkerMedium</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:textColorTertiary">@*com.android.vending:color/play_tertiary_settings_set</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="*com.android.vending:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.android.vending:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
