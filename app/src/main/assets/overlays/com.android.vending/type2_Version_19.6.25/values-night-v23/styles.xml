<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Base.V23.Theme.GoogleMaterial.Light" parent="@*com.android.vending:style/Base.V21.Theme.GoogleMaterial.Dark">
        <item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="LeftNavRedesignTheme" parent="@*com.android.vending:style/Theme.Replay.Apps">
        <item name="android:textColorPrimary">@*android:color/white</item>
        <item name="android:textAppearanceMedium">@*com.android.vending:style/DarkerMedium</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:textColorTertiary">@*com.android.vending:color/play_tertiary_settings_set</item>
        <item name="android:colorAccent">@*com.android.vending:color/apps_primary_dark</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="*com.android.vending:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.android.vending:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="*com.android.vending:delegateToSupportLibAlertDialog">true</item>
    </style>
    <style name="MainActivityTheme" parent="@*com.android.vending:style/FinskyTheme">
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="*com.android.vending:actionMenuTextColor">@*com.android.vending:color/play_fg_secondary</item>
        <item name="*com.android.vending:actionModeStyle">@*com.android.vending:style/FinskyActionModeStyle</item>
        <item name="*com.android.vending:preferenceTheme">@*com.android.vending:style/SettingsRedesignTheme</item>
        <item name="*com.android.vending:windowActionModeOverlay">true</item>
    </style>
    <style name="MainActivityThemeLauncher" parent="@*com.android.vending:style/MainActivityTheme">
        <item name="android:windowBackground">@*com.android.vending:drawable/splash_screen_initial</item>
        <item name="android:windowShowWallpaper">false</item>
    </style>
    <style name="PopupRedirectActivityThemeBottomSheet" parent="@*com.android.vending:style/PopupRedirectActivityTheme.BottomSheet">
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="*com.android.vending:colorPrimary">@*android:color/background_floating_material_dark</item>
        <item name="*com.android.vending:colorPrimaryDark">@*android:color/background_floating_material_dark</item>
    </style>
</resources>
