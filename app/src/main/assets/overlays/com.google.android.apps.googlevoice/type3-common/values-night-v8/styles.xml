<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Base.Theme.GoogleMaterial.Light" parent="@*com.google.android.apps.googlevoice:style/Base.V21.Theme.GoogleMaterial.Dark">
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="FloatingActionButtonStyle" parent="@*com.google.android.apps.googlevoice:style/Widget.GoogleMaterial.FloatingActionButton">
        <item name="*com.google.android.apps.googlevoice:tint">@*android:color/white</item>
        <item name="*com.google.android.apps.googlevoice:backgroundTint">@*com.google.android.apps.googlevoice:color/app_accent_color</item>
        <item name="*com.google.android.apps.googlevoice:rippleColor">@*android:color/ripple_material_dark</item>
    </style>
    <style name="HomeTheme" parent="@*com.google.android.apps.googlevoice:style/BaseTheme">
        <item name="android:actionModeBackground">@*android:color/background_floating_material_dark</item>
        <item name="*com.google.android.apps.googlevoice:windowActionModeOverlay">true</item>
        <item name="*com.google.android.apps.googlevoice:actionModeStyle">@*com.google.android.apps.googlevoice:style/ActionModeStyle</item>
        <item name="*com.google.android.apps.googlevoice:actionModeCloseDrawable">@*com.google.android.apps.googlevoice:drawable/action_mode_close</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
    </style>
    <style name="HomeTheme.Launcher" parent="@*com.google.android.apps.googlevoice:style/HomeTheme">
        <item name="android:windowBackground">@*com.google.android.apps.googlevoice:drawable/quantum_launchscreen_voice_2020q4</item>
	    <item name="android:windowShowWallpaper">false</item>
        <item name="android:windowIsTranslucent">false</item>
    </style>
    <style name="MediaView">
        <item name="*com.google.android.apps.googlevoice:gifOverlayIcon">@*com.google.android.apps.googlevoice:drawable/quantum_ic_gif_white_36</item>
        <item name="*com.google.android.apps.googlevoice:emptyViewIcon">@*com.google.android.apps.googlevoice:drawable/ic_missing_photo</item>
        <item name="*com.google.android.apps.googlevoice:errorIcon">@*com.google.android.apps.googlevoice:drawable/quantum_ic_broken_image_white_24</item>
        <item name="*com.google.android.apps.googlevoice:playOverlayIcon">@*com.google.android.apps.googlevoice:drawable/quantum_ic_play_circle_fill_white_48</item>
        <item name="*com.google.android.apps.googlevoice:photosphereOverlayIcon">@*com.google.android.apps.googlevoice:drawable/ov_lightcycle_32</item>
        <item name="*com.google.android.apps.googlevoice:gifOverlayBackgroundColor">@*com.google.android.apps.googlevoice:color/mediaview_gif_overlay_background_color</item>
        <item name="*com.google.android.apps.googlevoice:playOverlayBackgroundColor">@*android:color/background_dark</item>
        <item name="*com.google.android.apps.googlevoice:progressBarColor">@*com.google.android.apps.googlevoice:color/quantum_googblue</item>
        <item name="*com.google.android.apps.googlevoice:progressBarHeight">6.0dip</item>
        <item name="*com.google.android.apps.googlevoice:fadeInAnimationDurationMs">100</item>
        <item name="*com.google.android.apps.googlevoice:fadeOutAnimationDurationMs">80</item>
    </style>
    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.DeviceDefault.Light" />
    <style name="ProgressDialogWithSpinnerOnly" parent="@*android:style/Theme.Holo.Dialog">
        <item name="android:textColorPrimary">@*android:color/white</item>
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:alertDialogStyle">@*com.google.android.apps.googlevoice:style/CustomAlertDialogStyle</item>
        <item name="android:textSize">12.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="SignupTheme" parent="@*com.google.android.apps.googlevoice:style/BaseTheme">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowNoTitle">true</item>
        <item name="*com.google.android.apps.googlevoice:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.google.android.apps.googlevoice:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="ThemeOverlay.GoogleMaterial.Snackbar" parent="">
        <item name="*com.google.android.apps.googlevoice:colorOnSurface">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="VerificationTheme" parent="@*com.google.android.apps.googlevoice:style/BaseTheme">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="VoicePreferenceCategoryText" parent="@*com.google.android.apps.googlevoice:style/TextAppearance.GoogleMaterial.Overline">
        <item name="android:textColor">@*android:color/user_icon_default_white</item>
    </style>
    <style name="Widget.GoogleMaterial.Snackbar" parent="@*com.google.android.apps.googlevoice:style/Widget.MaterialComponents.Snackbar">
        <item name="*com.google.android.apps.googlevoice:actionTextColorAlpha">@*com.google.android.apps.googlevoice:dimen/design_snackbar_action_text_color_alpha</item>
        <item name="*com.google.android.apps.googlevoice:backgroundTint">@*android:color/background_floating_material_dark</item>
        <item name="*com.google.android.apps.googlevoice:enforceMaterialTheme">true</item>
        <item name="*com.google.android.apps.googlevoice:materialThemeOverlay">@*com.google.android.apps.googlevoice:style/ThemeOverlay.GoogleMaterial.Snackbar</item>
    </style>
    <style name="Widget.GoogleMaterial.Snackbar.TextView" parent="@*com.google.android.apps.googlevoice:style/Widget.MaterialComponents.Snackbar.TextView">
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:alpha">1.0</item>
    </style>
    <style name="Widget.MaterialComponents.AppBarLayout.Surface" parent="@*com.google.android.apps.googlevoice:style/Widget.Design.AppBarLayout">
        <item name="android:background">@*android:color/primary_material_dark</item>
    </style>
    <style name="Widget.MaterialComponents.Snackbar.TextView" parent="@*com.google.android.apps.googlevoice:style/Widget.AppCompat.TextView">
        <item name="android:textAppearance">@*com.google.android.apps.googlevoice:style/TextAppearance.GoogleMaterial.Body2</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="android:paddingTop">@*com.google.android.apps.googlevoice:dimen/design_snackbar_padding_vertical</item>
        <item name="android:paddingBottom">@*com.google.android.apps.googlevoice:dimen/design_snackbar_padding_vertical</item>
        <item name="android:layout_marginLeft">@*com.google.android.apps.googlevoice:dimen/mtrl_snackbar_message_margin_horizontal</item>
        <item name="android:layout_marginRight">@*com.google.android.apps.googlevoice:dimen/mtrl_snackbar_message_margin_horizontal</item>
        <item name="android:maxLines">@*com.google.android.apps.googlevoice:integer/design_snackbar_text_max_lines</item>
        <item name="android:alpha">@*com.google.android.apps.googlevoice:dimen/material_emphasis_high_type</item>
        <item name="android:textAlignment">viewStart</item>
    </style>
</resources>
