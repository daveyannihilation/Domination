<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ActionBarPopupTheme" parent="@*com.whatsapp:style/ThemeOverlay.AppCompat.Dark" />
    <style name="ActionBarTheme" parent="@*com.whatsapp:style/ThemeOverlay.AppCompat.Dark">
        <item name="android:textColorSecondary">@*android:color/white</item>
    </style>
    <style name="ActivityTheme" parent="@*com.whatsapp:style/Theme.AppCompat.NoActionBar">
        <item name="*com.whatsapp:windowActionBar">false</item>
        <item name="*com.whatsapp:windowNoTitle">true</item>
        <item name="*com.whatsapp:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.whatsapp:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="BusinessProfileButton" parent="@*com.whatsapp:style/Widget.AppCompat.Button.Borderless.Colored">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ff00796b</item>
        <item name="android:background">@null</item>
        <item name="android:textAllCaps">true</item>
    </style>
    <style name="DatePickerDialog" parent="@*com.whatsapp:style/ThemeOverlay.AppCompat.Dialog.Alert">
        <item name="android:headerBackground">#ff388e3c</item>
        <item name="android:datePickerStyle">@*com.whatsapp:style/SpinnerDatePicker</item>
        <item name="*com.whatsapp:colorControlActivated">@*com.whatsapp:color/accent</item>
        <item name="*com.whatsapp:colorControlHighlight">@*com.whatsapp:color/accent</item>
        <item name="*com.whatsapp:showTitle">false</item>
    </style>
    <style name="DescribeProblemInlineLink" parent="@*android:style/Widget.TextView">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ff1976d2</item>
    </style>
    <style name="FullScreenDialogSeparator">
        <item name="android:background">#ff388e3c</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">9.0dip</item>
    </style>
    <style name="List">
        <item name="android:scrollbarStyle">insideOverlay</item>
        <item name="android:fadingEdgeLength">8.0dip</item>
        <item name="android:drawSelectorOnTop">false</item>
        <item name="android:cacheColorHint">@*android:color/background_dark</item>
        <item name="android:divider">#00000000</item>
        <item name="android:dividerHeight">0.0px</item>
    </style>
    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.DeviceDefault.Light" />
    <style name="SettingsCategoryFooter" parent="@*com.whatsapp:style/Text.Description">
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:gravity">fill_horizontal</item>
        <item name="android:background">@*android:color/primary_dark_material_dark</item>
        <item name="android:paddingLeft">12.0dip</item>
        <item name="android:paddingTop">12.0dip</item>
        <item name="android:paddingRight">12.0dip</item>
        <item name="android:paddingBottom">12.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="Text.Bullet" parent="@*com.whatsapp:style/Text.Steps">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:paddingRight">4.0dip</item>
        <item name="android:text">@*com.whatsapp:string/contacts_help_bullet</item>
    </style>
    <style name="Text.FieldHeader" parent="@*com.whatsapp:style/Text">
        <item name="android:textSize">12.0sp</item>
        <item name="android:typeface">sans</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:gravity">left</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:paddingLeft">16.0dip</item>
        <item name="android:paddingRight">16.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowDx">1.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">0.0</item>
        <item name="android:lineSpacingMultiplier">1.25</item>
    </style>
    <style name="Theme.App.Black" parent="@*com.whatsapp:style/Theme.App">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="*com.whatsapp:actionBarStyle">@*com.whatsapp:style/Widget.Styled.ActionBar.GifVideoPreview</item>
        <item name="*com.whatsapp:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.whatsapp:colorPrimaryDark">@*android:color/transparent</item>
        <item name="*com.whatsapp:homeAsUpIndicator">@*com.whatsapp:drawable/ic_back</item>
    </style>
    <style name="Theme.App.ChatInfo" parent="@*com.whatsapp:style/Theme.App">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:backgroundTint">@*android:color/background_dark</item>
        <item name="*com.whatsapp:windowActionBar">false</item>
        <item name="*com.whatsapp:windowNoTitle">true</item>
        <item name="*com.whatsapp:actionOverflowButtonStyle">@*com.whatsapp:style/OverFlowWithShadow</item>
        <item name="*com.whatsapp:homeAsUpIndicator">@*com.whatsapp:drawable/ic_back_shadow</item>
    </style>
    <style name="Theme.App.Home" parent="@*com.whatsapp:style/Theme.App">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:fastScrollThumbDrawable">@*com.whatsapp:drawable/fastscroll_thumb_home</item>
        <item name="android:fastScrollTrackDrawable">@*com.whatsapp:drawable/fastscroll_track</item>
        <item name="*com.whatsapp:tabTitleStyle">@*com.whatsapp:style/TabTitle</item>
        <item name="*com.whatsapp:windowActionBar">false</item>
        <item name="*com.whatsapp:windowNoTitle">true</item>
    </style>
    <style name="Theme.App.StatusPlayback" parent="@*com.whatsapp:style/Theme.App">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="*com.whatsapp:windowActionBar">false</item>
        <item name="*com.whatsapp:windowNoTitle">true</item>
        <item name="*com.whatsapp:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.whatsapp:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="*com.whatsapp:colorAccent">@*com.whatsapp:color/accent</item>
    </style>
    <style name="Theme.Launcher" parent="@*com.whatsapp:style/Theme.App">
        <item name="android:windowBackground">@*com.whatsapp:drawable/splash</item>
        <item name="android:windowShowWallpaper">false</item>
    </style>
    <style name="Theme.PopupNotification" parent="@*com.whatsapp:style/Theme.App">
        <item name="android:windowBackground">@*android:color/primary_text_leanback_light</item>
        <item name="android:backgroundTint">@*android:color/transparent</item> 
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@*android:style/Animation.Dialog</item>
        <item name="*com.whatsapp:windowActionBar">false</item>
        <item name="*com.whatsapp:windowNoTitle">true</item>
    </style>
    <style name="Theme.RequestPermission" parent="@*com.whatsapp:style/Theme.App">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@*android:style/Animation.Dialog</item>
        <item name="*com.whatsapp:colorAccent">@*com.whatsapp:color/accent</item>
        <item name="*com.whatsapp:colorPrimaryDark">@*com.whatsapp:color/primary_dark_dimmed</item>
        <item name="*com.whatsapp:windowActionBar">false</item>
        <item name="*com.whatsapp:windowNoTitle">true</item>
    </style>
    <style name="Theme.Voip" parent="@*com.whatsapp:style/Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="*com.whatsapp:actionBarSize">@*com.whatsapp:dimen/actionbar_height</item>
        <item name="*com.whatsapp:actionBarStyle">@*com.whatsapp:style/Widget.Styled.ActionBar</item>
        <item name="*com.whatsapp:colorAccent">@*com.whatsapp:color/accent</item>
        <item name="*com.whatsapp:colorPrimary">@*com.whatsapp:color/primary_voip</item>
        <item name="*com.whatsapp:colorPrimaryDark">@*com.whatsapp:color/primary_dark_voip</item>
        <item name="*com.whatsapp:toolbarStyle">@*com.whatsapp:style/Widget.Styled.Toolbar</item>
    </style>
    <style name="Toolbar" parent="@*com.whatsapp:style/ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textAppearanceMedium">@*com.whatsapp:style/TextAppearanceMenuList</item>
        <item name="*com.whatsapp:colorControlNormal">@*android:color/white</item>
        <item name="*com.whatsapp:colorControlActivated">?*android:textColorSecondary</item>
    </style>
    <style name="Toolbar.Popup" parent="@*com.whatsapp:style/ThemeOverlay.AppCompat.Dark" />
    <style name="VoipDebugParam">
        <item name="android:textSize">11.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@*android:color/background_dark</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="Widget.Styled.ActionBar.Registration.White" parent="@*com.whatsapp:style/Widget.Styled.ActionBar">
        <item name="android:titleTextStyle">@*com.whatsapp:style/Theme.ActionBar.Registration.TitleTextStyle</item>
        <item name="*com.whatsapp:background">@*android:color/primary_material_dark</item>
        <item name="*com.whatsapp:elevation">4.0dip</item>
        <item name="*com.whatsapp:titleTextStyle">@*com.whatsapp:style/Theme.ActionBar.Registration.TitleTextStyle</item>
    </style>
</resources>
