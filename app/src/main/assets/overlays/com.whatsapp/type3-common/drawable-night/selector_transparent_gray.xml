<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ff555555" android:endColor="#ff555555" android:angle="90.0" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <gradient android:startColor="#ffe64a19" android:endColor="#fffbc02d" android:angle="90.0" android:centerColor="#fff57f17" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#00000000" android:endColor="#00000000" android:angle="90.0" />
        </shape>
    </item>
</selector>
