<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Platform.V25.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar" />
    <style name="SudAlertDialogTheme.Light" parent="@*android:style/Theme.DeviceDefault.Dialog.Alert" />
    <style name="Theme.SafetyHub" parent="@*com.google.android.apps.safetyhub:style/Theme.GoogleMaterial.DayNight.NoActionBar">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:clipChildren">false</item>
        <item name="android:clipToPadding">false</item>
        <item name="android:navigationBarColor">@*android:color/transparent</item>
        <item name="android:navigationBarDividerColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.apps.safetyhub:alertDialogTheme">@*com.google.android.apps.safetyhub:style/AlertDialogThemeOverlay</item>
        <item name="*com.google.android.apps.safetyhub:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="ToolbarNavButtonStyle" parent="@*com.google.android.apps.safetyhub:style/ThemeOverlay.AppCompat.ActionBar">
        <item name="android:background">@*android:color/primary_material_dark</item>
        <item name="android:colorControlNormal">@*com.google.android.apps.safetyhub:color/app_highlight</item>
    </style>
</resources>
