<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BottomNavStyle">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="*com.google.android.apps.wallpaper:itemBackground">?*android:selectableItemBackgroundBorderless</item>
        <item name="*com.google.android.apps.wallpaper:itemIconTint">@*com.google.android.apps.wallpaper:color/bottom_nav_item_color</item>
        <item name="*com.google.android.apps.wallpaper:itemTextAppearanceActive">@*com.google.android.apps.wallpaper:style/BottomNavTextAppearanceActive</item>
        <item name="*com.google.android.apps.wallpaper:itemTextAppearanceInactive">@*com.google.android.apps.wallpaper:style/BottomNavTextAppearance</item>
        <item name="*com.google.android.apps.wallpaper:itemTextColor">@*com.google.android.apps.wallpaper:color/bottom_nav_item_color</item>
    </style>
    <style name="CardView" parent="@*com.google.android.apps.wallpaper:style/Base.CardView">
        <item name="*com.google.android.apps.wallpaper:cardBackgroundColor">@*android:color/primary_material_dark</item>
    </style>
    <style name="CustomizationTheme" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:navigationBarDividerColor">@*android:color/transparent</item>
        <item name="*com.google.android.apps.wallpaper:actionButtonStyle">?*android:actionButtonStyle</item>
        <item name="*com.google.android.apps.wallpaper:actionMenuTextColor">?*android:actionMenuTextColor</item>
        <item name="*com.google.android.apps.wallpaper:colorControlHighlight">@*android:color/secondary_text_default_material_dark</item>
        <item name="*com.google.android.apps.wallpaper:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.google.android.apps.wallpaper:dialogPreferredPadding">24.0dip</item>
        <item name="*com.google.android.apps.wallpaper:selectableItemBackground">?*android:selectableItemBackground</item>
    </style>
    <style name="Platform.V21.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorHintInverse">#ffbbbbbb</item>
        <item name="android:textColorHint">#ffbbbbbb</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
        <item name="android:textColorLinkInverse">@*android:color/accent_material_dark</item>
    </style>
    <style name="Platform.V25.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar" />
    <style name="WallpaperPicker.BottomPaneStyle" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:textColorPrimary">@*android:color/white</item>
        <item name="android:textColorSecondary">@*android:color/secondary_text_default_material_dark</item>
        <item name="*com.google.android.apps.wallpaper:tabGravity">fill</item>
        <item name="*com.google.android.apps.wallpaper:tabIndicatorColor">@*android:color/white</item>
        <item name="*com.google.android.apps.wallpaper:tabMaxWidth">0.0dip</item>
        <item name="*com.google.android.apps.wallpaper:tabTextAppearance">@*com.google.android.apps.wallpaper:style/WallpaperPicker.Preview.TextAppearance.NoAllCaps</item>
    </style>
    <style name="WallpaperTheme" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:fitsSystemWindows">false</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:navigationBarDividerColor">@*android:color/transparent</item>
        <item name="*com.google.android.apps.wallpaper:actionBarSize">?*android:actionBarSize</item>
        <item name="*com.google.android.apps.wallpaper:colorControlActivated">@*android:color/white</item>
        <item name="*com.google.android.apps.wallpaper:colorControlHighlight">@color/ripple_material_dark</item>
        <item name="*com.google.android.apps.wallpaper:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.google.android.apps.wallpaper:dialogPreferredPadding">24.0dip</item>
        <item name="*com.google.android.apps.wallpaper:homeAsUpIndicator">@*com.google.android.apps.wallpaper:drawable/material_ic_arrow_back_black_24</item>
        <item name="*com.google.android.apps.wallpaper:selectableItemBackground">?*android:selectableItemBackground</item>
        <item name="*com.google.android.apps.wallpaper:toolbarNavigationButtonStyle">@*com.google.android.apps.wallpaper:style/Widget.AppCompat.Toolbar.Button.Navigation</item>
        <item name="*com.google.android.apps.wallpaper:windowActionBar">false</item>
        <item name="*com.google.android.apps.wallpaper:windowNoTitle">true</item>
    </style>
    <style name="WallpaperTheme.Preview" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:actionBarItemBackground">?*android:selectableItemBackgroundBorderless</item>
        <item name="android:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="android:statusBarColor">@*android:color/transparent</item>
        <item name="android:navigationBarColor">@*android:color/transparent</item>
        <item name="*com.google.android.apps.wallpaper:actionBarSize">?*android:actionBarSize</item>
        <item name="*com.google.android.apps.wallpaper:borderlessButtonStyle">@*com.google.android.apps.wallpaper:style/Widget.AppCompat.Button.Borderless</item>
        <item name="*com.google.android.apps.wallpaper:colorControlActivated">@*android:color/white</item>
        <item name="*com.google.android.apps.wallpaper:colorControlHighlight">@*android:color/ripple_material_dark</item>
        <item name="*com.google.android.apps.wallpaper:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.google.android.apps.wallpaper:dialogPreferredPadding">24.0dip</item>
        <item name="*com.google.android.apps.wallpaper:homeAsUpIndicator">@*com.google.android.apps.wallpaper:drawable/material_ic_arrow_back_black_24</item>
        <item name="*com.google.android.apps.wallpaper:selectableItemBackground">?*android:selectableItemBackground</item>
        <item name="*com.google.android.apps.wallpaper:toolbarNavigationButtonStyle">@*com.google.android.apps.wallpaper:style/Widget.AppCompat.Toolbar.Button.Navigation</item>
        <item name="*com.google.android.apps.wallpaper:windowActionBar">false</item>
        <item name="*com.google.android.apps.wallpaper:windowNoTitle">true</item>
    </style>
</resources>
