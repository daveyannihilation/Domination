<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="WallpaperTheme" parent="@*com.google.android.apps.wallpaper:style/Theme.AppCompat.Light.NoActionBar">
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.apps.wallpaper:colorControlActivated">@*android:color/white</item>
        <item name="*com.google.android.apps.wallpaper:colorControlNormal">@*com.google.android.apps.wallpaper:color/accent_color</item>
    </style>
</resources>
