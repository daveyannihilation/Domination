<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="LiveEventScoreCardBottomTeam">
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:backgroundTint">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="LiveEventScoreCardTopTeam">
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="android:background">@*android:color/background_dark</item>
        <item name="android:backgroundTint">@*android:color/background_dark</item>
    </style>
    <style name="PaletteLightsOut">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="*com.twitter.android:abstractColorCellBackground">@*android:color/primary_dark_material_dark</item>
        <item name="*com.twitter.android:abstractColorDeepGray">@*android:color/white</item>
        <item name="*com.twitter.android:abstractColorDivider">@*android:color/transparent</item>
        <item name="*com.twitter.android:abstractColorFadedGray">@*android:color/primary_dark_material_dark</item>
        <item name="*com.twitter.android:abstractColorFaintGray">@*android:color/background_floating_material_dark</item>
        <item name="*com.twitter.android:abstractColorHighlightBackground">@*android:color/dim_foreground_light</item>
        <item name="*com.twitter.android:abstractColorLightGray">@*android:color/transparent</item>
        <item name="*com.twitter.android:abstractColorLink">@*com.twitter.android:color/twitter_blue</item>
        <item name="*com.twitter.android:abstractColorMediumGray">@*android:color/white</item>
        <item name="*com.twitter.android:abstractColorText">@*android:color/white</item>
        <item name="*com.twitter.android:abstractColorUnread">@*android:color/button_material_dark</item>
    </style>
    <style name="TwitterBase.LightsOut" parent="@*com.twitter.android:style/PaletteLightsOut">
        <item name="*com.twitter.android:twitterColorBadgeVerified">@*android:color/white</item>
        <item name="*com.twitter.android:twitterColorComposerOverflowTextHighlight">@*com.twitter.android:color/medium_red</item>
        <item name="*com.twitter.android:twitterColorDeepGray30">@*android:color/secondary_text_default_material_dark</item>
        <item name="*com.twitter.android:twitterColorDeepGray50">@*android:color/secondary_text_default_material_dark</item>
        <item name="*com.twitter.android:twitterColorDrawerBackground">@*android:color/background_floating_material_dark</item>
        <item name="*com.twitter.android:twitterColorFocusedBg">@*android:color/dim_foreground_light</item>
        <item name="*com.twitter.android:twitterColorGlobalRippleSelector">@*android:color/secondary_text_default_material_dark</item>
        <item name="*com.twitter.android:twitterColorMediumGray30">@*android:color/background_dark</item>
        <item name="*com.twitter.android:twitterColorMediumGray50">@*android:color/secondary_text_default_material_dark</item>
        <item name="*com.twitter.android:twitterColorPlaceholderBg">@*android:color/primary_material_dark</item>
        <item name="*com.twitter.android:twitterColorPressed">@*android:color/dim_foreground_light</item>
        <item name="*com.twitter.android:twitterColorPressedOverlay">@*android:color/secondary_text_default_material_dark</item>
        <item name="*com.twitter.android:twitterColorSpeechBubble">@*android:color/button_material_dark</item>
        <item name="*com.twitter.android:twitterColorTextBlue">@*com.twitter.android:color/twitter_blue</item>
        <item name="*com.twitter.android:twitterColorTextDisabled">@*android:color/secondary_text_default_material_dark</item>
    </style>
</resources>
