<?xml version="1.0" encoding="utf-8"?>
<resources>


    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.DeviceDefault.Light" />





    <style name="CloudSetupPages">
        <item name="android:textColor">#ffffffff</item>
    </style>

    <style name="ContainerListStyle" parent="@*android:style/Widget.Material.ListView">
        <item name="android:divider">@*com.touchtype.swiftkey:drawable/material_pref_divider</item>
    </style>

    <style name="ContainerTheme.Authentication" parent="@*android:style/Theme.Material.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>

    <style name="ContainerTheme.StatsCard" parent="@*com.touchtype.swiftkey:style/ContainerTheme">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">4.0dip</item>
        <item name="android:layout_marginRight">4.0dip</item>
        <item name="android:layout_marginBottom">8.0dip</item>
    </style>

    <style name="ContainerTheme.TipWidgetAction" parent="@*com.touchtype.swiftkey:style/ContainerTheme">
        <item name="android:textSize">11.0sp</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:gravity">end</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="ContainerTheme.TipWidgetSummary" parent="@*com.touchtype.swiftkey:style/ContainerTheme">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffdddddd</item>
    </style>

    <style name="InstallerPolicy">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textColor">@*com.touchtype.swiftkey:color/cloud_sign_in_text</item>
        <item name="android:textColorLink">@*com.touchtype.swiftkey:color/installer_policy_link_text</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingTop">15.0dip</item>
        <item name="android:paddingBottom">15.0dip</item>
        <item name="android:text">16sp</item>
        <item name="android:fontFamily">sans-serif-thin</item>
    </style>
    <style name="InstallerSuccessText">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>

    <style name="NoActionBar" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
    </style>




    <style name="deviceDefaultThemeLightFullscreen" parent="@*android:style/Theme.DeviceDefault.NoActionBar.Fullscreen" />


    <style name="CloudSetupGrid.Text" parent="@*com.touchtype.swiftkey:style/CloudSetupGrid">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:layout_gravity">top</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="CloudSetupGrid.Title" parent="@*com.touchtype.swiftkey:style/CloudSetupGrid">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:layout_gravity">top</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>


    <style name="CloudSetupGrid.Description" parent="@*com.touchtype.swiftkey:style/CloudSetupGrid">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>


    <style name="ContainerTheme.InstallerSuccess" parent="@*com.touchtype.swiftkey:style/ContainerTheme">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="*com.touchtype.swiftkey:colorAccent">@*com.touchtype.swiftkey:color/swiftkey_cerf_darker</item>
    </style>
    <style name="ContainerTheme.Themes.Tile" parent="@*com.touchtype.swiftkey:style/ContainerTheme.Themes">
        <item name="*com.touchtype.swiftkey:cardBackgroundColor">@*android:color/background_floating_material_dark</item>
        <item name="*com.touchtype.swiftkey:cardCornerRadius">@*com.touchtype.swiftkey:dimen/theme_item_card_radius</item>
        <item name="*com.touchtype.swiftkey:cardElevation">@*com.touchtype.swiftkey:dimen/theme_item_card_elevation</item>
    </style>


    <style name="CalendarOnboardingButton" parent="@*com.touchtype.swiftkey:style/Widget.AppCompat.Button.Colored">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">100.0dip</item>
        <item name="android:layout_height">48.0dip</item>
        <item name="android:textAllCaps">true</item>
        <item name="*com.touchtype.swiftkey:backgroundTint">#ff1976d2</item>
    </style>


    <style name="ContainerTheme.AgeGate" parent="@*com.touchtype.swiftkey:style/ContainerTheme">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="*com.touchtype.swiftkey:colorAccent">#ff1976d2</item>
    </style>
    <style name="ContainerTheme.CloudSetup.AgeGate" parent="@*com.touchtype.swiftkey:style/ContainerTheme.CloudSetup">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:fontFamily">@*com.touchtype.swiftkey:string/product_font_regular</item>
        <item name="*com.touchtype.swiftkey:colorAccent">#ff1976d2</item>
    </style>
</resources>
