<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ContainerTheme" parent="@*com.touchtype.swiftkey:style/BaseContainerTheme">
        <item name="*com.touchtype.swiftkey:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="ContainerTheme.BackgroundImageEditor" parent="@*com.touchtype.swiftkey:style/ContainerTheme">
        <item name="*com.touchtype.swiftkey:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="DeleteMyAccountAgeGateButtonTheme" parent="@*com.touchtype.swiftkey:style/Widget.AppCompat.Button.Colored">
        <item name="android:disabledAlpha">1.0</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:colorButtonNormal">@*com.touchtype.swiftkey:color/swiftkey_kahn_darker</item>
        <item name="*com.touchtype.swiftkey:colorButtonNormal">@*com.touchtype.swiftkey:color/swiftkey_kahn_darker</item>
    </style>
    <style name="FancyPanelErrorButton" parent="@*com.touchtype.swiftkey:style/Widget.MaterialComponents.Button">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:paddingLeft">24.0dip</item>
        <item name="android:paddingRight">24.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:paddingStart">24.0dip</item>
        <item name="android:paddingEnd">24.0dip</item>
        <item name="*com.touchtype.swiftkey:backgroundTint">@*android:color/button_material_dark</item>
        <item name="*com.touchtype.swiftkey:rippleColor">@*android:color/secondary_text_default_material_dark</item>
    </style>
    <style name="FancyPanelErrorButtonWithIcon" parent="@*com.touchtype.swiftkey:style/Widget.MaterialComponents.Button.Icon">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:paddingLeft">24.0dip</item>
        <item name="android:paddingRight">24.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:paddingStart">24.0dip</item>
        <item name="android:paddingEnd">24.0dip</item>
        <item name="*com.touchtype.swiftkey:backgroundTint">@*android:color/button_material_dark</item>
        <item name="*com.touchtype.swiftkey:iconTint">@*android:color/white</item>
        <item name="*com.touchtype.swiftkey:rippleColor">@*android:color/white</item>
    </style>
    <style name="HandwritingCoachmarkButton">
        <item name="android:textSize">@*com.touchtype.swiftkey:dimen/hwr_unavailable_coachmark_button_text_size</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:background">@*android:color/transparent</item>
        <item name="android:padding">@*com.touchtype.swiftkey:dimen/hwr_unavailable_coachmark_button_padding</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">true</item>
    </style>
    <style name="OverlayCoachmarkButton" parent="@*com.touchtype.swiftkey:style/Widget.MaterialComponents.Button.TextButton">
        <item name="android:textSize">@*com.touchtype.swiftkey:dimen/overlay_coachmark_button_text_size</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">true</item>
        <item name="*com.touchtype.swiftkey:rippleColor">@*android:color/white</item>
    </style>
    <style name="Platform.V21.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorHintInverse">#ffbbbbbb</item>
        <item name="android:textColorHint">#ffbbbbbb</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
        <item name="android:textColorLinkInverse">@*android:color/accent_material_dark</item>
    </style>
    <style name="ToolbarMessagingButton" parent="@*com.touchtype.swiftkey:style/Widget.AppCompat.Button.Colored">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:layout_margin">5.0dip</item>
        <item name="android:maxLength">12</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="*com.touchtype.swiftkey:backgroundTint">@*com.touchtype.swiftkey:color/toolbar_messaging_background_tint</item>
    </style>
