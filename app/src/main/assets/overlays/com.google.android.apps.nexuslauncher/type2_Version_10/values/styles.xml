<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AllAppsEmptySearchBackground.Dark" parent="@*com.google.android.apps.nexuslauncher:style/AllAppsEmptySearchBackground">
        <item name="android:colorForeground">@*com.google.android.apps.nexuslauncher:color/all_apps_bg_hand_fill_dark</item>
        <item name="android:colorControlHighlight">#ffffffff</item>
        <item name="android:colorPrimary">@*android:color/primary_material_dark</item>
    </style>
    <style name="AppTheme.Dark" parent="@*com.google.android.apps.nexuslauncher:style/LauncherTheme.Dark">
        <item name="*com.google.android.apps.nexuslauncher:hotseatQsbFeedbackColor">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="AppTheme.Dark.DarkMainColor" parent="@*com.google.android.apps.nexuslauncher:style/LauncherTheme.Dark.DarkMainColor">
        <item name="*com.google.android.apps.nexuslauncher:hotseatQsbFeedbackColor">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="AppTheme.Dark.DarkText" parent="@*com.google.android.apps.nexuslauncher:style/LauncherTheme.Dark.DarkText">
        <item name="*com.google.android.apps.nexuslauncher:hotseatQsbFillColor">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="BaseLauncherTheme" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:colorEdgeEffect">@*android:color/secondary_text_default_material_dark</item>
        <item name="android:windowLayoutInDisplayCutoutMode">shortEdges</item>
        <item name="android:enforceStatusBarContrast">false</item>
        <item name="android:enforceNavigationBarContrast">false</item>
        <item name="*com.google.android.apps.nexuslauncher:folderTextColor">@*android:color/white</item>
    </style>
    <style name="LauncherTheme.Dark" parent="@*com.google.android.apps.nexuslauncher:style/LauncherTheme">
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColorSecondary">#ffffffff</item>
        <item name="android:textColorHint">#a0ffffff</item>
        <item name="android:textColorTertiary">#ccffffff</item>
        <item name="android:colorControlHighlight">#a0ffffff</item>
        <item name="android:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.google.android.apps.nexuslauncher:allAppsInterimScrimAlpha">102</item>
        <item name="*com.google.android.apps.nexuslauncher:allAppsNavBarScrimColor">#80000000</item>
        <item name="*com.google.android.apps.nexuslauncher:allAppsScrimColor">#33000000</item>
        <item name="*com.google.android.apps.nexuslauncher:folderDotColor">#ffffffff</item>
        <item name="*com.google.android.apps.nexuslauncher:folderFillColor">@*android:color/background_floating_material_dark</item>
        <item name="*com.google.android.apps.nexuslauncher:folderIconBorderColor">#ffffffff</item>
        <item name="*com.google.android.apps.nexuslauncher:folderTextColor">@*android:color/white</item>
        <item name="*com.google.android.apps.nexuslauncher:isMainColorDark">true</item>
        <item name="*com.google.android.apps.nexuslauncher:loadingIconColor">#99ffffff</item>
        <item name="*com.google.android.apps.nexuslauncher:popupColorPrimary">@*android:color/background_floating_material_dark</item>
        <item name="*com.google.android.apps.nexuslauncher:popupColorSecondary">@*android:color/background_floating_material_dark</item>
        <item name="*com.google.android.apps.nexuslauncher:popupColorTertiary">@*android:color/button_material_dark</item>
        <item name="*com.google.android.apps.nexuslauncher:widgetsTheme">@*com.google.android.apps.nexuslauncher:style/WidgetContainerTheme.Dark</item>
    </style>
    <style name="LauncherTheme.Dark.DarkMainColor" parent="@*com.google.android.apps.nexuslauncher:style/LauncherTheme.Dark">
        <item name="*com.google.android.apps.nexuslauncher:folderFillColor">@*android:color/background_floating_material_dark</item>
        <item name="*com.google.android.apps.nexuslauncher:folderTextColor">@*android:color/white</item>
    </style>
    <style name="LauncherTheme.Dark.DarkText" parent="@*com.google.android.apps.nexuslauncher:style/LauncherTheme.Dark">
        <item name="*com.google.android.apps.nexuslauncher:allAppsInterimScrimAlpha">25</item>
        <item name="*com.google.android.apps.nexuslauncher:folderFillColor">@*android:color/background_floating_material_dark</item>
        <item name="*com.google.android.apps.nexuslauncher:folderTextColor">@*android:color/white</item>
        <item name="*com.google.android.apps.nexuslauncher:isWorkspaceDarkText">false</item>
        <item name="*com.google.android.apps.nexuslauncher:workspaceAmbientShadowColor">@*android:color/transparent</item>
        <item name="*com.google.android.apps.nexuslauncher:workspaceKeyShadowColor">@*android:color/transparent</item>
        <item name="*com.google.android.apps.nexuslauncher:workspaceShadowColor">@*android:color/transparent</item>
        <item name="*com.google.android.apps.nexuslauncher:workspaceStatusBarScrim">@null</item>
        <item name="*com.google.android.apps.nexuslauncher:workspaceTextColor">#ffffffff</item>
    </style>
    <style name="WidgetContainerTheme.Dark" parent="@*com.google.android.apps.nexuslauncher:style/AppTheme.Dark">
        <item name="android:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="android:colorEdgeEffect">@*android:color/secondary_text_default_material_dark</item>
    </style>
</resources>
