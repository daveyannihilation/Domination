<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="@*com.google.android.apps.translate:style/AppBaseTheme">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@*com.google.android.apps.translate:color/status_bar_20pct_black</item>
    </style>
    <style name="AppTheme.Launcher" parent="@*com.google.android.apps.translate:style/AppTheme">
	    <item name="android:windowShowWallpaper">false</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowBackground">@*com.google.android.apps.translate:drawable/quantum_launchscreen_translate</item>
        <item name="*com.google.android.apps.translate:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:backgroundTint">@null</item>
    </style>
    <style name="Base.ThemeOverlay.AppCompat.Dark.ActionBar" parent="@*com.google.android.apps.translate:style/Base.ThemeOverlay.AppCompat.Dark">
        <item name="*com.google.android.apps.translate:colorControlNormal">@*android:color/white</item>
        <item name="*com.google.android.apps.translate:searchViewStyle">@*com.google.android.apps.translate:style/Widget.AppCompat.SearchView.ActionBar</item>
        <item name="android:backgroundTint">@null</item>
    </style>
    <style name="CardView" parent="@*com.google.android.apps.translate:style/Base.CardView">
        <item name="*com.google.android.apps.translate:cardBackgroundColor">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="ContinuousTranslationDarkTheme" parent="@*com.google.android.apps.translate:style/WhiteTheme">
        <item name="android:textColorPrimary">@*android:color/white</item>
        <item name="android:textColorSecondary">@*com.google.android.apps.translate:color/quantum_white_hint_text</item>
        <item name="*com.google.android.apps.translate:alertDialogTheme">@*com.google.android.apps.translate:style/AlertDialogDarkTheme</item>
        <item name="*com.google.android.apps.translate:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.google.android.apps.translate:dividerColor">@*android:color/transparent</item>
        <item name="*com.google.android.apps.translate:pickerTextColor">@*android:color/white</item>
        <item name="*com.google.android.apps.translate:tintColor">@*android:color/white</item>
        <item name="*com.google.android.apps.translate:windowActionBar">false</item>
    </style>
    <style name="ContinuousTranslationTheme" parent="@*com.google.android.apps.translate:style/WhiteTheme">
        <item name="android:textColorPrimary">@*android:color/white</item>
        <item name="*com.google.android.apps.translate:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.google.android.apps.translate:dividerColor">@*android:color/transparent</item>
        <item name="*com.google.android.apps.translate:pickerIconColor">@*android:color/white</item>
        <item name="*com.google.android.apps.translate:pickerTextColor">@*android:color/white</item>
        <item name="*com.google.android.apps.translate:tintColor">@*android:color/white</item>
    </style>
    <style name="OpticsDialogTheme" parent="@*com.google.android.apps.translate:style/Theme.GoogleMaterial.Dark.NoActionBar.Bridge">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowFullscreen">true</item>
        <item name="*com.google.android.apps.translate:colorAccent">@*com.google.android.apps.translate:color/primary_blue</item>
        <item name="*com.google.android.apps.translate:alertDialogTheme">@*com.google.android.apps.translate:style/AlertDialogDarkTheme</item>
    </style>
    <style name="Platform.V21.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorHintInverse">#ffbbbbbb</item>
        <item name="android:textColorHint">#ffbbbbbb</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
        <item name="android:textColorLinkInverse">@*android:color/accent_material_dark</item>
    </style>
    <style name="SimpleWhiteBgTheme" parent="@*com.google.android.apps.translate:style/AppBaseTheme">
        <item name="android:windowBackground">@*android:color/background_dark</item>
    </style>
    <style name="WhiteDialogTheme" parent="@*com.google.android.apps.translate:style/Theme.AppCompat.Dialog">
        <item name="*com.google.android.apps.translate:windowActionBar">false</item>
        <item name="*com.google.android.apps.translate:windowNoTitle">true</item>
        <item name="*com.google.android.apps.translate:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.google.android.apps.translate:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.apps.translate:colorControlNormal">@*android:color/secondary_text_default_material_dark</item>
        <item name="*com.google.android.apps.translate:titleTextColor">@*android:color/white</item>
    </style>
    <style name="WhiteTheme" parent="@*com.google.android.apps.translate:style/Theme.AppCompat.Light.NoActionBar">
        <item name="*com.google.android.apps.translate:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.google.android.apps.translate:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.apps.translate:colorAccent">@*com.google.android.apps.translate:color/primary_blue</item>
        <item name="*com.google.android.apps.translate:colorControlNormal">@*android:color/secondary_text_default_material_dark</item>
        <item name="*com.google.android.apps.translate:titleTextColor">@*android:color/white</item>
    </style>
    <style name="Widget.GoogleMaterial.Snackbar" parent="@*com.google.android.apps.translate:style/Widget.MaterialComponents.Snackbar">
        <item name="*com.google.android.apps.translate:actionTextColorAlpha">@*com.google.android.apps.translate:dimen/design_snackbar_action_text_color_alpha</item>
        <item name="*com.google.android.apps.translate:backgroundTint">@*android:color/background_floating_material_dark</item>
        <item name="*com.google.android.apps.translate:enforceMaterialTheme">true</item>
        <item name="*com.google.android.apps.translate:materialThemeOverlay">@*com.google.android.apps.translate:style/ThemeOverlay.GoogleMaterial.Snackbar</item>
    </style>
    <style name="Widget.GoogleMaterial.Snackbar.TextView" parent="@*com.google.android.apps.translate:style/Widget.MaterialComponents.Snackbar.TextView">
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="Widget.MaterialComponents.Snackbar.TextView" parent="@*com.google.android.apps.translate:style/Widget.AppCompat.TextView">
        <item name="android:textAppearance">@*com.google.android.apps.translate:style/TextAppearance.GoogleMaterial.Body2</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="android:paddingLeft">@*com.google.android.apps.translate:dimen/design_snackbar_padding_horizontal</item>
        <item name="android:paddingTop">@*com.google.android.apps.translate:dimen/design_snackbar_padding_vertical</item>
        <item name="android:paddingRight">@*com.google.android.apps.translate:dimen/design_snackbar_padding_horizontal</item>
        <item name="android:paddingBottom">@*com.google.android.apps.translate:dimen/design_snackbar_padding_vertical</item>
        <item name="android:maxLines">@*com.google.android.apps.translate:integer/design_snackbar_text_max_lines</item>
        <item name="android:alpha">@*com.google.android.apps.translate:dimen/material_emphasis_high_type</item>
        <item name="android:textAlignment">viewStart</item>
    </style>
    <style name="growthkit_feature_highlight_black">
        <item name="android:textColor">#ffffffff</item>
    </style>
</resources>
