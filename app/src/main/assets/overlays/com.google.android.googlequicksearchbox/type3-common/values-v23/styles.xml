<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AgendaHubActivity.Theme.Base" parent="@*com.google.android.googlequicksearchbox:style/Theme.AppCompat">
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="CardView" parent="@*com.google.android.googlequicksearchbox:style/Base.CardView">
        <item name="*com.google.android.googlequicksearchbox:cardBackgroundColor">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="OpaHQActivity.Theme.Base" parent="@*com.google.android.googlequicksearchbox:style/Theme.AppCompat">
        <item name="android:activityOpenEnterAnimation">@*com.google.android.googlequicksearchbox:anim/slide_in_bottom_hq</item>
        <item name="android:activityCloseExitAnimation">@*com.google.android.googlequicksearchbox:anim/slide_out_bottom_hq</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="*com.google.android.googlequicksearchbox:windowActionBar">false</item>
        <item name="*com.google.android.googlequicksearchbox:windowNoTitle">true</item>
    </style>
    <style name="StandaloneZeroState.Theme.Base" parent="@*com.google.android.googlequicksearchbox:style/Theme.Velvet">
        <item name="android:backgroundDimAmount">0.58</item>
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:popupMenuStyle">@*com.google.android.googlequicksearchbox:style/Opa.PopupMenu</item>
        <item name="android:textAppearanceLargePopupMenu">@*com.google.android.googlequicksearchbox:style/TextAppearance.AppCompat.Light.Widget.PopupMenu.Large</item>
        <item name="android:textAppearanceSmallPopupMenu">@*com.google.android.googlequicksearchbox:style/TextAppearance.AppCompat.Light.Widget.PopupMenu.Small</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="SudThemeGlifV2.Light" parent="@*com.google.android.googlequicksearchbox:style/SudThemeGlif.Light">
        <item name="android:colorBackground">@*android:color/background_dark</item>
        <item name="android:editTextStyle">@*com.google.android.googlequicksearchbox:style/SudEditText</item>
        <item name="android:alertDialogTheme">@*com.google.android.googlequicksearchbox:style/SudAlertDialogTheme.Light</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="*com.google.android.googlequicksearchbox:sucLightStatusBar">false</item>
        <item name="*com.google.android.googlequicksearchbox:sudBackgroundBaseColor">@*android:color/background_dark</item>
        <item name="*com.google.android.googlequicksearchbox:sudBackgroundPatterned">false</item>
        <item name="*com.google.android.googlequicksearchbox:sudDividerInsetEnd">@*com.google.android.googlequicksearchbox:dimen/sud_glif_margin_sides</item>
        <item name="*com.google.android.googlequicksearchbox:sudDividerInsetStart">@*com.google.android.googlequicksearchbox:dimen/sud_glif_margin_sides</item>
        <item name="*com.google.android.googlequicksearchbox:sudDividerInsetStartNoIcon">@*com.google.android.googlequicksearchbox:dimen/sud_glif_margin_sides</item>
        <item name="*com.google.android.googlequicksearchbox:sudEditTextBackgroundColor">@*com.google.android.googlequicksearchbox:color/sud_glif_edit_text_bg_light_color</item>
        <item name="*com.google.android.googlequicksearchbox:sudGlifHeaderGravity">center_horizontal</item>
        <item name="*com.google.android.googlequicksearchbox:sudScrollIndicators">bottom|top</item>
    </style>
    <style name="Theme.Gsa.Hub" parent="@*com.google.android.googlequicksearchbox:style/Theme.Gsa.Hub.Base">
        <item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="Theme.HotwordEnrollment.Glif.Suw" parent="@*com.google.android.googlequicksearchbox:style/Theme.HotwordEnrollment.Glif">
        <item name="android:statusBarColor">@*android:color/transparent</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="ViewerTheme" parent="@*com.google.android.googlequicksearchbox:style/Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@*com.google.android.googlequicksearchbox:drawable/splash_screen</item>
        <item name="android:windowAnimationStyle">@*com.google.android.googlequicksearchbox:style/ViewerAnimation.Activity</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>
    <style name="ZeroStateFabBottomSheetDialog" parent="@*com.google.android.googlequicksearchbox:style/Theme.Design.Light.BottomSheetDialog">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowAnimationStyle">@*com.google.android.googlequicksearchbox:style/Animation.MaterialComponents.BottomSheetDialog</item>
        <item name="android:statusBarColor">@*android:color/transparent</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="*com.google.android.googlequicksearchbox:bottomSheetStyle">@*com.google.android.googlequicksearchbox:style/ZeroStateFabBottomSheet</item>
    </style>
</resources>
