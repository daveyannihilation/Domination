<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@*android:color/button_material_dark" />
            <corners android:radius="@*com.google.android.googlequicksearchbox:dimen/shutterbutton_size" />
            <stroke android:width="5.0dip" android:color="#ffffffff" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ff1976d2" />
            <corners android:radius="@*com.google.android.googlequicksearchbox:dimen/shutterbutton_size" />
            <stroke android:width="5.0dip" android:color="#ffffffff" />
        </shape>
    </item>
</selector>
