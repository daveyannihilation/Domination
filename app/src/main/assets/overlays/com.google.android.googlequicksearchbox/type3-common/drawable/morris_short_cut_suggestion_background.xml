<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:height="@*com.google.android.googlequicksearchbox:dimen/morris_short_cut_suggestion_height">
        <shape android:shape="rectangle">
            <corners android:radius="@*com.google.android.googlequicksearchbox:dimen/morris_short_cut_suggestion_radius" />
            <solid android:color="@*android:color/background_floating_material_dark" />
        </shape>
    </item>
    <item android:gravity="center_horizontal" android:height="@*com.google.android.googlequicksearchbox:dimen/morris_short_cut_suggestion_image_background_size" android:width="@*com.google.android.googlequicksearchbox:dimen/morris_short_cut_suggestion_image_background_size" android:top="@*com.google.android.googlequicksearchbox:dimen/morris_short_cut_suggestion_image_padding_top">
        <shape android:shape="oval">
            <solid android:color="@*com.google.android.googlequicksearchbox:color/morris_short_cut_suggestion_image_background_color" />
        </shape>
    </item>
    <item>
        <ripple android:color="@*android:color/secondary_text_default_material_dark">
            <item android:id="@*android:id/mask">
                <shape android:shape="rectangle">
                    <corners android:radius="@*com.google.android.googlequicksearchbox:dimen/morris_short_cut_suggestion_radius" />
                    <solid android:color="@*android:color/secondary_text_default_material_dark" />
                </shape>
            </item>
        </ripple>
    </item>
</layer-list>
