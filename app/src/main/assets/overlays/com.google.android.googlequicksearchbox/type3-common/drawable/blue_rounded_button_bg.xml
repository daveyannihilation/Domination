<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <solid android:color="#ff1976d2" />
            <corners android:radius="@*com.google.android.googlequicksearchbox:dimen/rounded_button_radius" />
            <stroke android:width="1.0dip" android:color="@*android:color/white" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ff0d47a1" />
            <corners android:radius="@*com.google.android.googlequicksearchbox:dimen/rounded_button_radius" />
            <stroke android:width="1.0dip" android:color="@*android:color/white" />
        </shape>
    </item>
</selector>
