<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <scale android:drawable="@*android:color/secondary_text_default_material_dark" android:scaleWidth="100%" />
    </item>
    <item android:id="@*android:id/secondaryProgress">
        <shape android:shape="rectangle">
            <solid android:color="#40ffffff" />
        </shape>
    </item>
    <item android:id="@*android:id/progress">
        <scale android:drawable="@*com.google.android.googlequicksearchbox:color/google_blue700" android:scaleWidth="100%" />
    </item>
</layer-list>
