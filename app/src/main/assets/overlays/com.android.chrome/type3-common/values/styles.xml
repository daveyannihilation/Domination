<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Base.V7.Widget.AppCompat.AutoCompleteTextView" parent="@*android:style/Widget.AutoCompleteTextView">
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:background">@*com.android.chrome:drawable/abc_edit_text_material</item>
        <item name="android:dropDownSelector">@*com.android.chrome:drawable/abc_list_selector_holo_dark</item>
        <item name="android:popupBackground">@*com.android.chrome:drawable/abc_popup_background_mtrl_mult</item>
    </style>

    <style name="Base.V7.Widget.AppCompat.EditText" parent="@*android:style/Widget.EditText">
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:background">@*com.android.chrome:drawable/abc_edit_text_material</item>
    </style>
    <style name="Base.Widget.AppCompat.ActionBar.TabText" parent="">
        <item name="android:textAppearance">@*com.android.chrome:style/TextAppearance.AppCompat.Medium</item>
        <item name="android:textSize">12.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:maxWidth">180.0dip</item>
        <item name="android:maxLines">2</item>
        <item name="*com.android.chrome:textAllCaps">true</item>
    </style>

    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:dropDownListViewStyle">@*com.android.chrome:style/Base.Widget.AppCompat.ListView.DropDown</item>
    </style>
    <style name="Base.Widget.AppCompat.ListView.DropDown" parent="@*android:style/Widget.Material.ListView.DropDown">
      <item name="android:backgroundTint">@*android:color/background_floating_material_dark</item>
    </style> 

    <style name="PreferenceTextAppearanceMedium">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="ButtonBar" parent="@*android:style/Holo.ButtonBar.AlertDialog">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:divider">?android:dividerVertical</item>
        <item name="android:showDividers">middle</item>
        <item name="android:dividerPadding">0.0dip</item>
    </style>
    <style name="ButtonBarButton" parent="@*android:style/Widget.Holo.Button">
        <item name="android:background">@*com.android.chrome:drawable/abc_item_background_holo_dark</item>
        <item name="android:paddingLeft">4.0dip</item>
        <item name="android:paddingRight">4.0dip</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="WebNotificationButton" parent="@*android:style/Widget.DeviceDefault.Button.Borderless.Small">
        <item name="android:textColor">#ffdddddd</item>
    </style>

    <style name="ContextualSearchTextViewLayout">
        <item name="android:layout_gravity">bottom</item>
        <item name="android:background">@*android:color/background_dark</item>
        <item name="android:paddingLeft">53.0dip</item>
        <item name="android:paddingRight">53.0dip</item>
        <item name="android:visibility">invisible</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="ContextualSearchContextTextView">
        <item name="android:textSize">@*com.android.chrome:dimen/text_size_large</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:ellipsize">none</item>
        <item name="android:fadingEdge">horizontal</item>
        <item name="android:fadingEdgeLength">30.0dip</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:requiresFadingEdge">horizontal</item>
    </style>




    <style name="ContextualSearchCaptionTextView">
        <item name="android:textAppearance">@*com.android.chrome:style/BlackBody</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginLeft">7.0dip</item>
        <item name="android:layout_marginRight">7.0dip</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="Widget.Design.BottomSheet.Modal" parent="@*android:style/Widget">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:elevation">@*com.android.chrome:dimen/design_bottom_sheet_modal_elevation</item>
        <item name="*com.android.chrome:behavior_peekHeight">auto</item>
        <item name="*com.android.chrome:behavior_hideable">true</item>
        <item name="*com.android.chrome:behavior_skipCollapsed">false</item>
    </style>


    <style name="SearchActivityTheme" parent="@*com.android.chrome:style/MainTheme">
        <item name="android:windowBackground">@*android:color/background_dark</item>
    </style>


    <style name="FullscreenWhiteActivityTheme" parent="@*com.android.chrome:style/FullscreenWhite">
        <item name="*com.android.chrome:windowActionBar">false</item>
        <item name="android:backgroundTint">@*android:color/primary_material_dark</item>
    </style>


    <style name="TabbedModeTheme" parent="@*com.android.chrome:style/MainTheme">
        <item name="android:windowBackground">@*android:color/background_dark</item>
    </style>


    <style name="GvrDialogTheme" parent="@*android:style/Theme.Holo.Dialog">
        <item name="android:windowBackground">@*android:color/transparent</item>
    </style>


    <style name="FullscreenWhiteBase" parent="@*com.android.chrome:style/Theme.AppCompat">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:textColorLink">@*com.android.chrome:color/light_active_color</item>
        <item name="*com.android.chrome:windowNoTitle">true</item>
        <item name="*com.android.chrome:colorPrimary">@*com.android.chrome:color/light_active_color</item>
        <item name="*com.android.chrome:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="*com.android.chrome:colorAccent">@*com.android.chrome:color/light_active_color</item>
    </style>
</resources>
