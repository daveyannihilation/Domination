<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="RE_AlertDialog_Black" parent="@*com.speedsoftware.rootexplorer:style/Theme.AppCompat.Dialog.Alert">
        <item name="android:windowBackground">@*android:color/background_floating_material_dark</item>
        <item name="*com.speedsoftware.rootexplorer:colorAccent">@*android:color/accent_material_dark</item>
    </style>
    <style name="RE_Application_Black" parent="@*com.speedsoftware.rootexplorer:style/RE_Application_Dark">
        <item name="android:colorBackground">@*android:color/background_dark</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="*com.speedsoftware.rootexplorer:actionBarIconColor">@*android:color/white</item>
        <item name="*com.speedsoftware.rootexplorer:colorAccent">@*android:color/accent_material_dark</item>
        <item name="*com.speedsoftware.rootexplorer:colorAccent2">@*android:color/accent_material_dark</item>
        <item name="*com.speedsoftware.rootexplorer:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.speedsoftware.rootexplorer:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="*com.speedsoftware.rootexplorer:folderColor">@*android:color/accent_material_dark</item>
        <item name="*com.speedsoftware.rootexplorer:listFilenameColor">@*android:color/white</item>
        <item name="*com.speedsoftware.rootexplorer:listItemBackgroundColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.speedsoftware.rootexplorer:listItemDividerColor">@*android:color/transparent</item>
        <item name="*com.speedsoftware.rootexplorer:preferenceTheme">@*com.speedsoftware.rootexplorer:style/PreferenceThemeOverlay</item>
    </style>
    <style name="RE_Dialog_Black" parent="@*com.speedsoftware.rootexplorer:style/Theme.IOSched.Dark.Dialog">
        <item name="android:colorBackground">@*android:color/background_floating_material_dark</item>
        <item name="android:windowNoTitle">true</item>
        <item name="*com.speedsoftware.rootexplorer:colorAccent">@*android:color/accent_material_dark</item>
        <item name="*com.speedsoftware.rootexplorer:colorAccent2">@*android:color/accent_material_dark</item>
        <item name="*com.speedsoftware.rootexplorer:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.speedsoftware.rootexplorer:colorPrimaryDark">@*android:color/background_floating_material_dark</item>
        <item name="*com.speedsoftware.rootexplorer:folderColor">@*android:color/accent_material_dark</item>
    </style>
    <style name="Widget.Design.FloatingActionButton" parent="@*android:style/Widget">
        <item name="android:background">@*com.speedsoftware.rootexplorer:drawable/design_fab_background</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="*com.speedsoftware.rootexplorer:backgroundTint">@*android:color/button_material_dark</item>
        <item name="*com.speedsoftware.rootexplorer:borderWidth">@*com.speedsoftware.rootexplorer:dimen/design_fab_border_width</item>
        <item name="*com.speedsoftware.rootexplorer:elevation">@*com.speedsoftware.rootexplorer:dimen/design_fab_elevation</item>
        <item name="*com.speedsoftware.rootexplorer:fabSize">auto</item>
        <item name="*com.speedsoftware.rootexplorer:hideMotionSpec">@*com.speedsoftware.rootexplorer:animator/design_fab_hide_motion_spec</item>
        <item name="*com.speedsoftware.rootexplorer:hoveredFocusedTranslationZ">@*com.speedsoftware.rootexplorer:dimen/design_fab_translation_z_hovered_focused</item>
        <item name="*com.speedsoftware.rootexplorer:maxImageSize">@*com.speedsoftware.rootexplorer:dimen/design_fab_image_size</item>
        <item name="*com.speedsoftware.rootexplorer:pressedTranslationZ">@*com.speedsoftware.rootexplorer:dimen/design_fab_translation_z_pressed</item>
        <item name="*com.speedsoftware.rootexplorer:rippleColor">@*android:color/secondary_text_default_material_dark</item>
        <item name="*com.speedsoftware.rootexplorer:showMotionSpec">@*com.speedsoftware.rootexplorer:animator/design_fab_show_motion_spec</item>
    </style>
</resources>
