<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Base.TextAppearance.AppCompat.Widget.Button.Inverse" parent="@*android:style/TextAppearance.Material.Widget.Button" />
    <style name="CardView" parent="@*com.google.android.markup:style/Base.CardView">
        <item name="*com.google.android.markup:cardBackgroundColor">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="MarkupTheme" parent="@*com.google.android.markup:style/Theme.AppCompat">
        <item name="android:homeAsUpIndicator">@*com.google.android.markup:drawable/ic_home</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.markup:actionBarStyle">@*com.google.android.markup:style/MarkupActionBarStyle</item>
        <item name="*com.google.android.markup:actionButtonStyle">@*com.google.android.markup:style/MarkupActionButton</item>
        <item name="*com.google.android.markup:actionMenuTextColor">@*com.google.android.markup:color/markup_accent</item>
        <item name="*com.google.android.markup:colorAccent">@*com.google.android.markup:color/markup_accent</item>
        <item name="*com.google.android.markup:colorPrimary">@*com.google.android.markup:color/markup_primary</item>
        <item name="*com.google.android.markup:colorPrimaryDark">@*com.google.android.markup:color/markup_primary_dark</item>
    </style>
</resources>
