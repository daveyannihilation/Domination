<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="DockSettingTheme.NoActionBar" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:colorBackground">@*android:color/background_dark</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="DockSettingTheme.RenameDialogTheme" parent="@*android:style/Theme.DeviceDefault.Dialog.Alert">
        <item name="android:windowBackground">@*com.google.android.apps.dreamliner:drawable/alert_dialog_background</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:windowMinWidthMajor">@*com.google.android.apps.dreamliner:dimen/dialog_min_width_major</item>
        <item name="android:windowMinWidthMinor">@*com.google.android.apps.dreamliner:dimen/dialog_min_width_minor</item>
    </style>
    <style name="DockSettingTheme.ToolbarTheme" parent="@*com.google.android.apps.dreamliner:style/ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="android:colorBackground">@*android:color/primary_material_dark</item>
        <item name="android:actionButtonStyle">@*com.google.android.apps.dreamliner:style/DockSettingTheme.ActionButtonStyle</item>
    </style>
    <style name="SuwThemeGlifV3.Light.Dreamliner" parent="@*com.google.android.apps.dreamliner:style/SuwThemeGlifV3.Light">
        <item name="android:colorBackground">@*android:color/background_dark</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:statusBarColor">@*android:color/transparent</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>
</resources>
