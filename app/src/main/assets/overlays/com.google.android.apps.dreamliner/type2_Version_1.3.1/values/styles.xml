<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="DockSettingTheme.NoActionBar" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:colorBackground">@*android:color/background_dark</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.apps.dreamliner:colorControlActivated">@*com.google.android.apps.dreamliner:color/dark_color_accent</item>
    </style>
    <style name="DockSettingTheme.RenameDialogTheme" parent="@*android:style/Theme.DeviceDefault.Dialog.Alert">
        <item name="android:windowBackground">@*com.google.android.apps.dreamliner:drawable/alert_dialog_background</item>
        <item name="android:windowMinWidthMajor">@*com.google.android.apps.dreamliner:dimen/dialog_min_width_major</item>
        <item name="android:windowMinWidthMinor">@*com.google.android.apps.dreamliner:dimen/dialog_min_width_minor</item>
    </style>
    <style name="DockSettingTheme.TimePickerDialogTheme" parent="@*android:style/Theme.DeviceDefault.Dialog.Alert">
        <item name="android:windowMinWidthMajor">@*com.google.android.apps.dreamliner:dimen/dialog_min_width_major</item>
        <item name="android:windowMinWidthMinor">@*com.google.android.apps.dreamliner:dimen/dialog_min_width_minor</item>
    </style>
    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.DeviceDefault.Light" />
    <style name="SuwGlifCardContainer">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:elevation">@*com.google.android.apps.dreamliner:dimen/suw_glif_card_elevation</item>
    </style>
    <style name="SuwThemeGlifV2" parent="@*com.google.android.apps.dreamliner:style/SuwThemeGlif">
        <item name="android:colorBackground">@*com.google.android.apps.dreamliner:color/suw_glif_background_color_dark</item>
        <item name="android:editTextStyle">@*com.google.android.apps.dreamliner:style/SuwEditText</item>
        <item name="android:alertDialogTheme">@*com.google.android.apps.dreamliner:style/SuwAlertDialogTheme</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="*com.google.android.apps.dreamliner:suwDividerInsetEnd">@*com.google.android.apps.dreamliner:dimen/suw_glif_margin_sides</item>
        <item name="*com.google.android.apps.dreamliner:suwDividerInsetStart">@*com.google.android.apps.dreamliner:dimen/suw_glif_margin_sides</item>
        <item name="*com.google.android.apps.dreamliner:suwDividerInsetStartNoIcon">@*com.google.android.apps.dreamliner:dimen/suw_glif_margin_sides</item>
        <item name="*com.google.android.apps.dreamliner:suwEditTextBackgroundColor">@*com.google.android.apps.dreamliner:color/suw_glif_edit_text_bg_light_color</item>
        <item name="*com.google.android.apps.dreamliner:suwGlifHeaderGravity">center_horizontal</item>
        <item name="*com.google.android.apps.dreamliner:suwScrollIndicators">bottom|top</item>
        <item name="*com.google.android.apps.dreamliner:suwBackgroundPatterned">false</item>
        <item name="*com.google.android.apps.dreamliner:suwBackgroundBaseColor">@*android:color/background_dark</item>
    </style>
    <style name="alertDialogStyle" parent="@*android:style/Theme.DeviceDefault.Dialog.Alert">
        <item name="android:textColorPrimary">@*android:color/white</item>
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:background">@*com.google.android.apps.dreamliner:drawable/alert_dialog_background</item>
        <item name="android:windowMinWidthMajor">@*com.google.android.apps.dreamliner:dimen/alert_dialog_min_width_major</item>
        <item name="android:windowMinWidthMinor">@*com.google.android.apps.dreamliner:dimen/alert_dialog_min_width_minor</item>
    </style>
    <style name="main_battery">
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:fontFamily">google-sans-medium</item>
        <item name="android:autoSizeMinTextSize">14.0sp</item>
        <item name="android:autoSizeMaxTextSize">16.0sp</item>
    </style>
</resources>
