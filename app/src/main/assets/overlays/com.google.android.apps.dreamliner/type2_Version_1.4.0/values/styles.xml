<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CardView" parent="@*com.google.android.apps.dreamliner:style/Base.CardView">
        <item name="*com.google.android.apps.dreamliner:cardBackgroundColor">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="DockSettingTheme.NoActionBar" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:colorBackground">@*android:color/background_dark</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.apps.dreamliner:colorControlActivated">@*com.google.android.apps.dreamliner:color/alert_dialog_button_text</item>
    </style>
    <style name="DockSettingTheme.RenameDialogTheme" parent="@*android:style/Theme.DeviceDefault.Dialog.Alert">
        <item name="android:windowBackground">@*com.google.android.apps.dreamliner:drawable/alert_dialog_background</item>
        <item name="android:windowMinWidthMajor">@*com.google.android.apps.dreamliner:dimen/dialog_min_width_major</item>
        <item name="android:windowMinWidthMinor">@*com.google.android.apps.dreamliner:dimen/dialog_min_width_minor</item>
    </style>
    <style name="DockSettingTheme.TimePickerDialogTheme" parent="@*android:style/Theme.DeviceDefault.Dialog.Alert">
        <item name="android:windowMinWidthMajor">@*com.google.android.apps.dreamliner:dimen/dialog_min_width_major</item>
        <item name="android:windowMinWidthMinor">@*com.google.android.apps.dreamliner:dimen/dialog_min_width_minor</item>
    </style>
    <style name="Platform.V21.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorHintInverse">#ffbbbbbb</item>
        <item name="android:textColorHint">#ffbbbbbb</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
        <item name="android:textColorLinkInverse">@*android:color/accent_material_dark</item>
    </style>
    <style name="Platform.V25.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar" />
    <style name="SudAlertDialogTheme.Light" parent="@*android:style/Theme.DeviceDefault.Dialog.Alert" />
    <style name="SudGlifCardContainer">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:elevation">@*com.google.android.apps.dreamliner:dimen/sud_glif_card_elevation</item>
    </style>
    <style name="SudThemeGlifV2" parent="@*com.google.android.apps.dreamliner:style/SudThemeGlif">
        <item name="android:colorBackground">@*android:color/background_dark</item>
        <item name="android:editTextStyle">@*com.google.android.apps.dreamliner:style/SudEditText</item>
        <item name="android:alertDialogTheme">@*com.google.android.apps.dreamliner:style/SudAlertDialogTheme</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="*com.google.android.apps.dreamliner:sucLightStatusBar">false</item>
        <item name="*com.google.android.apps.dreamliner:sudBackgroundBaseColor">@*android:color/background_dark</item>
        <item name="*com.google.android.apps.dreamliner:sudBackgroundPatterned">false</item>
        <item name="*com.google.android.apps.dreamliner:sudDividerInsetEnd">@*com.google.android.apps.dreamliner:dimen/sud_glif_margin_sides</item>
        <item name="*com.google.android.apps.dreamliner:sudDividerInsetStart">@*com.google.android.apps.dreamliner:dimen/sud_glif_margin_sides</item>
        <item name="*com.google.android.apps.dreamliner:sudDividerInsetStartNoIcon">@*com.google.android.apps.dreamliner:dimen/sud_glif_margin_sides</item>
        <item name="*com.google.android.apps.dreamliner:sudEditTextBackgroundColor">@*com.google.android.apps.dreamliner:color/sud_glif_edit_text_bg_dark_color</item>
        <item name="*com.google.android.apps.dreamliner:sudGlifHeaderGravity">center_horizontal</item>
        <item name="*com.google.android.apps.dreamliner:sudScrollIndicators">bottom|top</item>
    </style>
    <style name="SudThemeGlifV2.Light" parent="@*com.google.android.apps.dreamliner:style/SudThemeGlif">
        <item name="android:colorBackground">@*android:color/background_dark</item>
        <item name="android:editTextStyle">@*com.google.android.apps.dreamliner:style/SudEditText</item>
        <item name="android:alertDialogTheme">@*com.google.android.apps.dreamliner:style/SudAlertDialogTheme</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="*com.google.android.apps.dreamliner:sucLightStatusBar">false</item>
        <item name="*com.google.android.apps.dreamliner:sudBackgroundBaseColor">@*android:color/background_dark</item>
        <item name="*com.google.android.apps.dreamliner:sudBackgroundPatterned">false</item>
        <item name="*com.google.android.apps.dreamliner:sudDividerInsetEnd">@*com.google.android.apps.dreamliner:dimen/sud_glif_margin_sides</item>
        <item name="*com.google.android.apps.dreamliner:sudDividerInsetStart">@*com.google.android.apps.dreamliner:dimen/sud_glif_margin_sides</item>
        <item name="*com.google.android.apps.dreamliner:sudDividerInsetStartNoIcon">@*com.google.android.apps.dreamliner:dimen/sud_glif_margin_sides</item>
        <item name="*com.google.android.apps.dreamliner:sudEditTextBackgroundColor">@*com.google.android.apps.dreamliner:color/sud_glif_edit_text_bg_dark_color</item>
        <item name="*com.google.android.apps.dreamliner:sudGlifHeaderGravity">center_horizontal</item>
        <item name="*com.google.android.apps.dreamliner:sudScrollIndicators">bottom|top</item>
    </style>
    <style name="SudThemeGlifV3.Light" parent="@*com.google.android.apps.dreamliner:style/SudBaseThemeGlifV3.Light">
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.apps.dreamliner:sucLightSystemNavBar">false</item>
    </style>
    <style name="alertDialogStyle" parent="@*android:style/Theme.DeviceDefault.Dialog.Alert">
        <item name="android:textColorPrimary">@*android:color/white</item>
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:background">@*com.google.android.apps.dreamliner:drawable/alert_dialog_background</item>
        <item name="android:windowMinWidthMajor">@*com.google.android.apps.dreamliner:dimen/alert_dialog_min_width_major</item>
        <item name="android:windowMinWidthMinor">@*com.google.android.apps.dreamliner:dimen/alert_dialog_min_width_minor</item>
    </style>
</resources>
