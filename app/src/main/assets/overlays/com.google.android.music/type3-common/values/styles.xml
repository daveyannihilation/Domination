<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BaseMusicActivityTheme" parent="@*com.google.android.music:style/Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:gridViewStyle">@*com.google.android.music:style/MusicGridView</item>
        <item name="android:listViewStyle">@*com.google.android.music:style/MusicListView</item>
        <item name="*com.google.android.music:drawerArrowStyle">@*com.google.android.music:style/MusicDrawerArrowStyle</item>
        <item name="*com.google.android.music:windowActionBarOverlay">true</item>
        <item name="*com.google.android.music:actionBarPopupTheme">@*com.google.android.music:style/ThemeOverlay.AppCompat.Light</item>
        <item name="*com.google.android.music:actionBarDivider">@null</item>
        <item name="*com.google.android.music:dialogTheme">@*com.google.android.music:style/MusicDialogTheme</item>
        <item name="*com.google.android.music:selectableItemBackground">@*com.google.android.music:drawable/white_highlight</item>
        <item name="*com.google.android.music:colorPrimary">@*com.google.android.music:color/app_color_primary</item>
        <item name="*com.google.android.music:colorPrimaryDark">@*com.google.android.music:color/app_color_secondary</item>
