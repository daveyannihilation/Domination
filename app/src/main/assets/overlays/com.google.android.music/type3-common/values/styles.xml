        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:fadingEdgeLength">0.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.0dip</item>
        <item name="android:cacheColorHint">@*android:color/background_floating_material_dark</item>
        <item name="android:divider">@null</item>
        <item name="android:choiceMode">none</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="PlayDrawerProfileEmailText" parent="@*com.google.android.music:style/PlayPlainText">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="PlayDrawerProfileNameText" parent="@*com.google.android.music:style/PlayMediumPlainText">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="PlayDrawerSelectedText" parent="@*com.google.android.music:style/PlayDrawerBaseText">
        <item name="android:textStyle">bold</item>
        <item name="android:background">#ff555555</item>
        <item name="android:foreground">@*com.google.android.music:drawable/play_highlight_overlay_light</item>
    </style>
    <style name="PlayHeaderListTabScrollview">
        <item name="android:background">#ffe64a19</item>
    </style>
    <style name="PlaySearchPlate">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:paddingLeft">@*com.google.android.music:dimen/play_search_plate_padding_side</item>
        <item name="android:paddingRight">@*com.google.android.music:dimen/play_search_plate_padding_side</item>
    </style>
    <style name="SecondaryTitle" parent="@*android:style/TextAppearance.Small">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:singleLine">true</item>
        <item name="android:fontFamily">sans-serif-light</item>
    </style>
    <style name="SoundSearchButton" parent="@*com.google.android.music:style/PlayMediumBoldText">
        <item name="android:textSize">@*com.google.android.music:dimen/sound_search_button_text_size</item>
        <item name="android:textColor">#ffe64a19</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@*android:color/button_material_dark</item>
        <item name="android:paddingLeft">@*com.google.android.music:dimen/sound_search_button_padding</item>
        <item name="android:paddingRight">@*com.google.android.music:dimen/sound_search_button_padding</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@*com.google.android.music:dimen/sound_search_button_height</item>
    </style>
    <style name="Theme.AppInvite.Preview" parent="@*com.google.android.music:style/Theme.AppInvite.Preview.Base">
        <item name="android:colorBackground">@*android:color/background_floating_material_dark</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="Theme.AppInvite.Preview.Base" parent="@*android:style/Theme.Holo.DialogWhenLarge.NoActionBar" />
