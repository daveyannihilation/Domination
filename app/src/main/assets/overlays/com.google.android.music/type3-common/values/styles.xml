        <item name="android:textColor">#ffffffff</item>
        <item name="android:background">@*com.google.android.music:drawable/tutorial_transparent_button_bg</item>
    </style>
    <style name="Tutorial.Button.Secondary" parent="@*com.google.android.music:style/Tutorial.Button">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:background">@*com.google.android.music:drawable/tutorial_transparent_button_bg</item>
    </style>
