    <style name="DialogSlideUpDownAnim" parent="@*android:style/Theme.Material.Dialog">
        <item name="android:windowAnimationStyle">@*com.google.android.music:style/BottomDialogAnimation</item>
    </style>
    <style name="FamilyOnboarding.NoThanks" parent="@*com.google.android.music:style/Tutorial.Button.Secondary">
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:paddingLeft">@*com.google.android.music:dimen/family_template_padding_medium</item>
        <item name="android:paddingRight">@*com.google.android.music:dimen/family_template_padding_medium</item>
        <item name="android:layout_margin">@*com.google.android.music:dimen/family_template_padding_medium</item>
    </style>
    <style name="FamilyOnboarding.Title" parent="@*com.google.android.music:style/FamilyOnboarding">
        <item name="android:textSize">@*com.google.android.music:dimen/family_onboarding_title_text_size</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:paddingLeft">@*com.google.android.music:dimen/family_onboarding_title_left_padding</item>
        <item name="android:paddingBottom">@*com.google.android.music:dimen/family_onboarding_title_bottom_padding</item>
    </style>
    <style name="FsiMessageActionButton" parent="@*com.google.android.music:style/Widget.AppCompat.Button.Colored">
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@*com.google.android.music:drawable/fsi_action_button_background_selector</item>
        <item name="android:paddingTop">@*com.google.android.music:dimen/default_padding_large</item>
        <item name="android:paddingBottom">@*com.google.android.music:dimen/default_padding_large</item>
        <item name="android:layout_marginBottom">@*com.google.android.music:dimen/default_padding</item>
        <item name="android:minWidth">@*com.google.android.music:dimen/fsi_action_button_min_width</item>
        <item name="android:minHeight">@*com.google.android.music:dimen/fsi_action_button_min_height</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">@*com.google.android.music:font/roboto_medium_family</item>
    </style>
    <style name="MusicActivityTheme.Launcher" parent="@*com.google.android.music:style/MusicActivityTheme">
        <item name="android:windowBackground">@*com.google.android.music:drawable/splashscreen_logo</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowShowWallpaper">false</item>
    </style>
    <style name="MusicCardNumber.Dark" parent="@*com.google.android.music:style/PlayCardTitle">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:background">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="MusicSettingsActionBar.TitleTextStyle" parent="@*com.google.android.music:style/MusicActionBar.TitleTextStyle">
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.DeviceDefault.Light" />
