<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ActionBar" parent="@*android:style/Widget.Material.ActionBar.Solid" />
    <style name="CalendarGmTheme.NoActionBar" parent="@*com.google.android.calendar:style/CalendarGmTheme.NoActionBar.Base">
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="CalendarGmTheme.NoActionBar.LaunchScreen" parent="@*com.google.android.calendar:style/CalendarGmTheme.NoActionBar">
        <item name="android:navigationBarColor">@*android:color/primary_text_leanback_light</item>
        <item name="android:windowBackground">@*com.google.android.calendar:drawable/quantum_launchscreen_calendar</item>
        <item name="android:windowShowWallpaper">false</item>
        <item name="android:windowIsTranslucent">false</item>
    </style>
    <style name="DetailsTheme_v21" parent="@*com.google.android.calendar:style/DetailsThemeCommon">
        <item name="android:windowIsFloating">@*com.google.android.calendar:bool/not_show_event_info_full_screen</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="EditTextToolbarGm" parent="@*com.google.android.calendar:style/DefaultToolbar">
        <item name="android:background">@*android:color/primary_material_dark</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingStart">4.0dip</item>
        <item name="*com.google.android.calendar:contentInsetStart">60.0dip</item>
        <item name="*com.google.android.calendar:contentInsetStartWithNavigation">60.0dip</item>
        <item name="*com.google.android.calendar:titleTextAppearance">@*com.google.android.calendar:style/TextAppearance.Toolbar.GoogleMaterial</item>
        <item name="*com.google.android.calendar:navigationIcon">@*com.google.android.calendar:drawable/quantum_gm_ic_arrow_back_vd_theme_24</item>
        <item name="*com.google.android.calendar:navigationContentDescription">@*com.google.android.calendar:string/accessibility_navigation_icon</item>
    </style>
    <style name="FindTimeSubTitle">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:paddingLeft">@*com.google.android.calendar:dimen/find_time_filter_header_horizontal_padding</item>
        <item name="android:paddingRight">@*com.google.android.calendar:dimen/find_time_filter_header_horizontal_padding</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@*com.google.android.calendar:dimen/find_time_filter_header_height</item>
    </style>
    <style name="FlatMaterialButtonGm.Active" parent="@*com.google.android.calendar:style/FlatMaterialButtonGm">
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="FlatMaterialButtonGm.LeftAction" parent="@*com.google.android.calendar:style/FlatMaterialButtonGm">
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:layout_marginStart">0.0dip</item>
        <item name="*com.google.android.calendar:backgroundTint">@*com.google.android.calendar:color/calendar_blue_highlight</item>
    </style>
    <style name="GrooveButtonBorderless" parent="@*com.google.android.calendar:style/Widget.AppCompat.Button.Borderless">
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="GrooveCreationWizardButton" parent="@*android:style/Widget.Holo.Button.Borderless">
        <item name="android:textSize">@*com.google.android.calendar:dimen/groove_wizard_button_text_size</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:textColorHint">#40ffffff</item>
        <item name="android:gravity">start|center</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textAlignment">viewStart</item>
        <item name="android:paddingStart">@*com.google.android.calendar:dimen/groove_selection_padding_horizontal</item>
    </style>
    <style name="GrooveCreationWizardTitle" parent="@*android:style/Widget.Holo.TextView">
        <item name="android:textSize">@*com.google.android.calendar:dimen/groove_selection_title_text_size</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:paddingTop">@*com.google.android.calendar:dimen/groove_selection_title_padding_top</item>
        <item name="android:paddingBottom">@*com.google.android.calendar:dimen/groove_selection_title_padding_bottom</item>
        <item name="android:focusable">true</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:textAlignment">viewStart</item>
        <item name="android:paddingStart">@*com.google.android.calendar:dimen/groove_selection_padding_horizontal</item>
    </style>
    <style name="GrooveScheduleWaitingText" parent="@*android:style/Widget.Holo.TextView">
        <item name="android:textSize">@*com.google.android.calendar:dimen/groove_selection_title_text_size</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="GrooveSummaryText" parent="@*android:style/Widget.Holo.TextView">
        <item name="android:textSize">@*com.google.android.calendar:dimen/groove_summary_text_text_size</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="TextAppearance.EditEvent" parent="@*com.google.android.calendar:style/TextAppearance">
        <item name="android:textAppearance">?*android:textAppearanceMedium</item>
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">#ffdddddd</item>
    </style>
    <style name="Widget.Design.BottomSheet.Modal" parent="@*android:style/Widget">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:elevation">@*com.google.android.calendar:dimen/design_bottom_sheet_modal_elevation</item>
        <item name="*com.google.android.calendar:behavior_peekHeight">auto</item>
        <item name="*com.google.android.calendar:behavior_hideable">true</item>
        <item name="*com.google.android.calendar:behavior_skipCollapsed">false</item>
        <item name="*com.google.android.calendar:shapeAppearanceOverlay">@null</item>
        <item name="*com.google.android.calendar:enforceMaterialTheme">false</item>
        <item name="*com.google.android.calendar:backgroundTint">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="Widget.GoogleMaterial.FloatingActionButton" parent="@*com.google.android.calendar:style/Widget.MaterialComponents.FloatingActionButton">
        <item name="*com.google.android.calendar:rippleColor">@*android:color/ripple_material_dark</item>
        <item name="*com.google.android.calendar:elevation">@*com.google.android.calendar:dimen/google_fab_elevation</item>
        <item name="*com.google.android.calendar:tint">@*com.google.android.calendar:color/google_blue500</item>
        <item name="*com.google.android.calendar:hoveredFocusedTranslationZ">@*com.google.android.calendar:dimen/google_fab_translation_z_hovered_focused</item>
        <item name="*com.google.android.calendar:pressedTranslationZ">@*com.google.android.calendar:dimen/google_fab_translation_z_pressed</item>
        <item name="*com.google.android.calendar:shapeAppearance">@null</item>
        <item name="*com.google.android.calendar:shapeAppearanceOverlay">@*com.google.android.calendar:style/ShapeAppearanceOverlay.GoogleMaterial.FloatingActionButton</item>
        <item name="*com.google.android.calendar:enforceMaterialTheme">true</item>
        <item name="*com.google.android.calendar:backgroundTint">@*android:color/button_material_dark</item>
    </style>
    <style name="Widget.GoogleMaterial.ProductLockupView.Dark" parent="@*com.google.android.calendar:style/Widget.GoogleMaterial.ProductLockupView">
        <item name="*com.google.android.calendar:productNameTextColor">@*android:color/white</item>
        <item name="*com.google.android.calendar:logoColor">light</item>
    </style>
    <style name="WidgetChip_Background_Narrow_Appearance">
        <item name="android:background">@*android:color/background_dark</item>
    </style>
    <style name="growthkit_feature_highlight_black">
        <item name="android:textColor">#ffffffff</item>
    </style>
</resources>
