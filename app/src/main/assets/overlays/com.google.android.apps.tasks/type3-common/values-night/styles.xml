<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BottomAppBar" parent="@*com.google.android.apps.tasks:style/Widget.GoogleMaterial.BottomAppBar">
        <item name="*com.google.android.apps.tasks:backgroundTint">@*android:color/background_floating_material_dark</item>
        <item name="*com.google.android.apps.tasks:backgroundTintMode">src_over</item>
        <item name="*com.google.android.apps.tasks:fabCradleMargin">5.0dip</item>
        <item name="*com.google.android.apps.tasks:fabCradleRoundedCornerRadius">8.0dip</item>
    </style>
    <style name="CardView" parent="@*com.google.android.apps.tasks:style/Base.CardView">
        <item name="*com.google.android.apps.tasks:cardBackgroundColor">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="DatePickerStyle" parent="@*android:style/Widget.Material.DatePicker">
        <item name="android:headerBackground">@*android:color/background_floating_material_dark</item>
        <item name="android:headerMonthTextAppearance">@*com.google.android.apps.tasks:style/DatePickerMonthTextAppearance</item>
    </style>
    <style name="DatePickerTheme" parent="@*com.google.android.apps.tasks:style/ThemeOverlay.GoogleMaterial.Dialog">
        <item name="android:textColorPrimaryInverse">@*android:color/white</item>
        <item name="android:windowBackground">@*com.google.android.apps.tasks:drawable/tasks_dialog_background</item>
        <item name="android:buttonBarButtonStyle">@*com.google.android.apps.tasks:style/Widget.GoogleMaterial.Button.TextButton</item>
        <item name="android:datePickerStyle">@*com.google.android.apps.tasks:style/DatePickerStyle</item>
        <item name="*com.google.android.apps.tasks:colorAccent">@*com.google.android.apps.tasks:color/colorAccent</item>
        <item name="*com.google.android.apps.tasks:colorControlActivated">@*com.google.android.apps.tasks:color/colorAccent</item>
        <item name="*com.google.android.apps.tasks:colorControlHighlight">@*com.google.android.apps.tasks:color/colorAccent</item>
    </style>
    <style name="Style.GrowthKit.Headline5" parent="@*com.google.android.apps.tasks:style/TextAppearance.GoogleMaterial.Headline5">
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="Style.GrowthKit.Subhead1" parent="@*com.google.android.apps.tasks:style/TextAppearance.GoogleMaterial.Subhead1">
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="TimePickerTheme" parent="@*com.google.android.apps.tasks:style/ThemeOverlay.GoogleMaterial.Dialog">
        <item name="android:textColorPrimaryInverse">@*android:color/white</item>
        <item name="android:windowBackground">@*com.google.android.apps.tasks:drawable/tasks_dialog_background</item>
        <item name="android:buttonBarButtonStyle">@*com.google.android.apps.tasks:style/Widget.GoogleMaterial.Button.TextButton</item>
        <item name="android:timePickerStyle">@*com.google.android.apps.tasks:style/TimePickerStyle</item>
        <item name="*com.google.android.apps.tasks:colorAccent">@*com.google.android.apps.tasks:color/colorAccent</item>
        <item name="*com.google.android.apps.tasks:colorControlActivated">@*com.google.android.apps.tasks:color/colorAccent</item>
        <item name="*com.google.android.apps.tasks:colorControlHighlight">@*com.google.android.apps.tasks:color/colorBackground</item>
    </style>
    <style name="Widget.GoogleMaterial.Snackbar" parent="@*com.google.android.apps.tasks:style/Widget.MaterialComponents.Snackbar">
        <item name="*com.google.android.apps.tasks:actionTextColorAlpha">@*com.google.android.apps.tasks:dimen/design_snackbar_action_text_color_alpha</item>
        <item name="*com.google.android.apps.tasks:backgroundTint">@*android:color/background_floating_material_dark</item>
        <item name="*com.google.android.apps.tasks:enforceMaterialTheme">true</item>
        <item name="*com.google.android.apps.tasks:materialThemeOverlay">@*com.google.android.apps.tasks:style/ThemeOverlay.GoogleMaterial.Snackbar</item>
    </style>
    <style name="Widget.GoogleMaterial.Snackbar.TextView" parent="@*com.google.android.apps.tasks:style/Widget.MaterialComponents.Snackbar.TextView">
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="Widget.MaterialComponents.Snackbar.TextView" parent="@*com.google.android.apps.tasks:style/Widget.AppCompat.TextView">
        <item name="android:textAppearance">@*com.google.android.apps.tasks:style/TextAppearance.GoogleMaterial.Body2</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="android:paddingLeft">@*com.google.android.apps.tasks:dimen/design_snackbar_padding_horizontal</item>
        <item name="android:paddingTop">@*com.google.android.apps.tasks:dimen/design_snackbar_padding_vertical</item>
        <item name="android:paddingRight">@*com.google.android.apps.tasks:dimen/design_snackbar_padding_horizontal</item>
        <item name="android:paddingBottom">@*com.google.android.apps.tasks:dimen/design_snackbar_padding_vertical</item>
        <item name="android:maxLines">@*com.google.android.apps.tasks:integer/design_snackbar_text_max_lines</item>
        <item name="android:alpha">@*com.google.android.apps.tasks:dimen/material_emphasis_high_type</item>
        <item name="android:textAlignment">viewStart</item>
    </style>
</resources>
