<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#4d000000" />
            <corners android:radius="@*com.instagram.android:dimen/button_corner_radius" />
            <stroke android:width="1.0dip" android:color="@*android:color/white" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@*android:color/transparent" />
            <corners android:radius="@*com.instagram.android:dimen/button_corner_radius" />
            <stroke android:width="1.0dip" android:color="@*android:color/secondary_text_default_material_dark" />
        </shape>
    </item>
</selector>
