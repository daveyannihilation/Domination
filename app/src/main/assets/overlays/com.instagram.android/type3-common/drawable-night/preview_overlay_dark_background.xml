<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <selector>
            <item android:state_pressed="true">
                <shape android:shape="oval">
                    <stroke android:width="1.0px" android:color="#66ffffff" />
                </shape>
            </item>
            <item>
                <shape android:shape="oval">
                    <stroke android:width="1.0px" android:color="#33ffffff" />
                </shape>
            </item>
        </selector>
    </item>
    <item>
        <inset android:insetLeft="1.0px" android:insetRight="1.0px" android:insetTop="1.0px" android:insetBottom="1.0px">
            <selector>
                <item android:state_pressed="true">
                    <shape android:shape="oval">
                        <solid android:color="#ff555555" />
                    </shape>
                </item>
                <item android:state_activated="true">
                    <shape android:shape="oval">
                        <solid android:color="#bf1976d2" />
                    </shape>
                </item>
                <item>
                    <shape android:shape="oval">
                        <solid android:color="@*android:color/button_material_dark" />
                    </shape>
                </item>
            </selector>
        </inset>
    </item>
</layer-list>
