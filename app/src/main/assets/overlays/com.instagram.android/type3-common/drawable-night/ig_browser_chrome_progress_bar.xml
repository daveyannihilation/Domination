<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <shape android:thickness="@*com.instagram.android:dimen/ig_browser_chrome_progress_bar_height">
            <solid android:color="@*android:color/secondary_text_default_material_dark" />
        </shape>
    </item>
    <item android:id="@*android:id/progress">
        <clip>
            <shape android:thickness="@*com.instagram.android:dimen/ig_browser_chrome_progress_bar_height">
                <solid android:color="@*com.instagram.android:color/blue_5" />
            </shape>
        </clip>
    </item>
</layer-list>
