<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AgendaButton.Light" parent="@*com.Slack:style/AgendaButton">
        <item name="android:textColor">@*android:color/white</item>
        <item name="*com.Slack:backgroundTint">@*android:color/button_material_dark</item>
        <item name="*com.Slack:colorControlHighlight">@*com.Slack:color/sk_lilypad_green</item>
        <item name="*com.Slack:strokeColor">@*android:color/white</item>
        <item name="*com.Slack:strokeWidth">1.0dip</item>
    </style>
    <style name="AndroidThemeColorAccentYellow">
        <item name="android:colorAccent">#fffbc02d</item>
    </style>
    <style name="BlackActivityTheme" parent="@style/NoActionBarActivityTheme">
        <item name="android:windowBackground">@*android:color/background_dark</item>
    </style>
    <style name="CounterText" parent="@*com.Slack:style/TextAppearance.Design.Counter.Overflow">
        <item name="android:textSize">@*com.Slack:dimen/app_dialog_text_size_small</item>
        <item name="android:textColor">@*android:color/secondary_text_default_material_dark</item>
        <item name="android:fontFamily">@*com.Slack:font/lato_regular</item>
    </style>
    <style name="DrawerActivityTheme.Launcher" parent="@*com.Slack:style/DrawerActivityTheme">
        <item name="android:windowBackground">@*com.Slack:drawable/launch_screen</item>
        <item name="android:statusBarColor">@*android:color/primary_text_leanback_light</item>
        <item name="android:windowShowWallpaper">false</item>
    </style>
    <style name="NoActionBarActivityTheme" parent="@*com.Slack:style/Theme.MaterialComponents.DayNight.NoActionBar.Bridge">
        <item name="android:colorBackground">@*android:color/background_floating_material_dark</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:fontFamily">@*com.Slack:font/lato_family</item>
        <item name="*com.Slack:actionModeBackground">@*android:color/background_floating_material_dark</item>
        <item name="*com.Slack:actionModeCloseDrawable">@*com.Slack:drawable/ic_back_24dp</item>
        <item name="*com.Slack:actionModeStyle">@*com.Slack:style/ActionModeTheme</item>
        <item name="*com.Slack:actionOverflowButtonStyle">@*com.Slack:style/Widget.ActionButton.Overflow</item>
        <item name="*com.Slack:alertDialogTheme">@*com.Slack:style/AlertDialogTheme</item>
        <item name="*com.Slack:bottomSheetDialogTheme">@*com.Slack:style/SlackModalBottomSheetDialog</item>
        <item name="*com.Slack:bottomSheetStyle">@*com.Slack:style/SlackBottomSheet</item>
        <item name="*com.Slack:cardViewStyle">@*com.Slack:style/SlackCardStyle</item>
        <item name="*com.Slack:colorAccent">@*com.Slack:color/colorAccent</item>
        <item name="*com.Slack:panelBackground">@*android:color/background_dark</item>
        <item name="*com.Slack:textAppearanceButton">@*com.Slack:style/TextAppearance.AppCompat.Button</item>
        <item name="*com.Slack:windowActionModeOverlay">true</item>
    </style>
    <style name="Platform.V21.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorHintInverse">#ffbbbbbb</item>
        <item name="android:textColorHint">#ffbbbbbb</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
        <item name="android:textColorLinkInverse">@*android:color/accent_material_dark</item>
    </style>
    <style name="SignInFlatWhiteButton" parent="@*com.Slack:style/SlackKit.Button.Large.Primary">
        <item name="*com.Slack:backgroundTint">@*android:color/button_material_dark</item>
        <item name="*com.Slack:rippleColor">@*com.Slack:color/colorControlHighlight</item>
    </style>
    <style name="SlackBottomSheet" parent="@*com.Slack:style/Widget.MaterialComponents.BottomSheet.Modal">
        <item name="*com.Slack:backgroundTint">@*android:color/background_floating_material_dark</item>
        <item name="*com.Slack:behavior_fitToContents">true</item>
        <item name="*com.Slack:behavior_hideable">true</item>
        <item name="*com.Slack:shapeAppearance">@*com.Slack:style/ShapeAppearance.Slack.BottomSheet</item>
        <item name="*com.Slack:shapeAppearanceOverlay">@*com.Slack:style/ShapeAppearance.Slack.BottomSheet</item>
    </style>
    <style name="SlackKit.Button.Large.White" parent="@*com.Slack:style/SlackKitButton.Large">
        <item name="android:textColor">@*com.Slack:color/selector_button_black</item>
        <item name="*com.Slack:backgroundTint">@*android:color/button_material_dark</item>
    </style>
    <style name="SlackKit.Button.Small.White" parent="@*com.Slack:style/SlackKitButton.Small">
        <item name="android:textColor">@*com.Slack:color/selector_button_black</item>
        <item name="*com.Slack:backgroundTint">@*android:color/button_material_dark</item>
    </style>
    <style name="StatusCardStyle">
        <item name="android:layout_marginLeft">8.0dip</item>
        <item name="android:layout_marginRight">8.0dip</item>
        <item name="*com.Slack:cardBackgroundColor">@*android:color/background_floating_material_dark</item>
        <item name="*com.Slack:cardCornerRadius">3.0dip</item>
    </style>
    <style name="ThemeOverlayColorAccentRed">
        <item name="android:colorAccent">#ffd32f2f</item>
    </style>
    <style name="Widget.MaterialComponents.Snackbar.TextView" parent="@*com.Slack:style/Widget.AppCompat.TextView">
        <item name="android:textAppearance">@*com.Slack:style/TextAppearance.MaterialComponents.Body2</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="android:paddingLeft">@*com.Slack:dimen/design_snackbar_padding_horizontal</item>
        <item name="android:paddingTop">@*com.Slack:dimen/design_snackbar_padding_vertical</item>
        <item name="android:paddingRight">@*com.Slack:dimen/design_snackbar_padding_horizontal</item>
        <item name="android:paddingBottom">@*com.Slack:dimen/design_snackbar_padding_vertical</item>
        <item name="android:maxLines">@*com.Slack:integer/design_snackbar_text_max_lines</item>
        <item name="android:alpha">@*com.Slack:dimen/material_emphasis_high_type</item>
        <item name="android:textAlignment">viewStart</item>
    </style>
</resources>
