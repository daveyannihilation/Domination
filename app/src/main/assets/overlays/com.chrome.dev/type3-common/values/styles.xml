<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Base.V7.Widget.AppCompat.AutoCompleteTextView" parent="@*android:style/Widget.AutoCompleteTextView">
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
        <item name="android:textColor">#ffdddddd</item>
    </style>

    <style name="Base.V7.Widget.AppCompat.EditText" parent="@*android:style/Widget.EditText">
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
        <item name="android:textColor">#ffdddddd</item>
    </style>
    <style name="Base.Widget.AppCompat.ActionBar.TabText" parent="">
        <item name="android:textColor">#ffffffff</item>
    </style>

    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.DeviceDefault.Light" />

    <style name="PreferenceTextAppearanceMedium">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="ButtonBar" parent="@*android:style/Holo.ButtonBar.AlertDialog">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:divider">?android:dividerVertical</item>
        <item name="android:showDividers">middle</item>
        <item name="android:dividerPadding">0.0dip</item>
    </style>
    <style name="ButtonBarButton" parent="@*android:style/Widget.Holo.Button">
        <item name="android:background">?android:selectableItemBackground</item>
        <item name="android:paddingStart">4.0dip</item>
        <item name="android:paddingEnd">4.0dip</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="WebNotificationButton" parent="@*android:style/Widget.DeviceDefault.Button.Borderless.Small">
        <item name="android:textColor">@*com.chrome.dev:color/secondary_text_default_material_light</item>
        <item name="android:background">@*com.chrome.dev:drawable/web_notification_button_background</item>
    </style>


    <style name="ContextualSearchTextViewLayout">
        <item name="android:layout_gravity">bottom</item>
        <item name="android:background">@*android:color/background_dark</item>
        <item name="android:paddingStart">53.0dip</item>
        <item name="android:paddingEnd">53.0dip</item>
        <item name="android:visibility">invisible</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="ContextualSearchTextView">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="ContextualSearchContextTextView">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:ellipsize">none</item>
        <item name="android:fadingEdge">horizontal</item>
        <item name="android:fadingEdgeLength">30.0dip</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:requiresFadingEdge">horizontal</item>
    </style>

    <style name="ContextualSearchCaptionTextView">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:background">@*android:color/background_dark</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginStart">7.0dip</item>
        <item name="android:layout_marginEnd">7.0dip</item>
        <item name="android:singleLine">true</item>
    </style>


    <style name="FullscreenWhiteBase" parent="@*com.chrome.dev:style/Theme.AppCompat.Light">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
        <item name="*com.chrome.dev:windowNoTitle">true</item>
        <item name="android:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="android:colorAccent">@*android:color/accent_material_dark</item>
    </style>


    <style name="SearchActivityTheme" parent="@*com.chrome.dev:style/MainTheme">
        <item name="android:windowBackground">@*android:color/background_dark</item>
    </style>

    <style name="Base.Widget.AppCompat.ListView.DropDown" parent="@*com.chrome.dev:style/Base.Widget.AppCompat.ListView">
        <item name="android:divider">@null</item>
        <item name="android:backgroundTint">@*android:color/background_floating_material_dark</item>
    </style>


    <style name="GvrDialogTheme" parent="@*android:style/Theme.Material.Dialog" />


    <style name="AutoCompleteTextView.LowEnd" parent="@*android:style/Widget.Material.AutoCompleteTextView">
        <item name="android:popupBackground">@*com.chrome.dev:drawable/popup_background_shadow</item>
    </style>
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabText" parent="@*android:style/Widget.Material.ActionBar.TabText" />
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabText.Inverse" parent="@*android:style/Widget.Material.ActionBar.TabText" />
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabView" parent="@*android:style/Widget.Material.ActionBar.TabView" />
    <style name="Base.Widget.AppCompat.Light.PopupMenu" parent="@*android:style/Widget.Material.PopupMenu" />
    <style name="CardView" parent="@*com.chrome.dev:style/Base.CardView">
        <item name="*com.chrome.dev:cardBackgroundColor">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="ListPopupWindow.LowEnd" parent="@*android:style/Widget.Material.ListPopupWindow">
        <item name="android:popupBackground">@*com.chrome.dev:drawable/popup_background_shadow</item>
    </style>
    <style name="PopupWindow.LowEnd" parent="@*android:style/Widget.Material.PopupWindow">
        <item name="android:popupBackground">@*com.chrome.dev:drawable/popup_background_shadow</item>
    </style>
    <style name="PreferencesDialogTheme" parent="@*android:style/Theme.Material.Dialog.Alert">
        <item name="android:colorAccent">@*com.chrome.dev:color/pref_accent_color</item>
    </style>
    <style name="Spinner.LowEnd" parent="@*android:style/Widget.Material.Spinner">
        <item name="android:popupBackground">@*com.chrome.dev:drawable/popup_background_shadow</item>
    </style>
</resources>
