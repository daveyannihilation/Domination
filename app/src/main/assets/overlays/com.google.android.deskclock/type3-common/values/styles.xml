<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ExpandableFab">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:ellipsize">none</item>
        <item name="android:singleLine">true</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">@*com.google.android.deskclock:font/google_sans</item>
    </style>
    <style name="Platform.AppCompat" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
    </style>
    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
    </style>
    <style name="Theme.DeskClock" parent="@*com.google.android.deskclock:style/Theme.GoogleMaterial.Dark.NoActionBar.Bridge">
        <item name="android:colorBackground">@*android:color/background_dark</item>
        <item name="android:textColorSecondary">@*android:color/secondary_text_default_material_dark</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:statusBarColor">@*android:color/transparent</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:timePickerStyle">@*com.google.android.deskclock:style/Widget.TimePicker</item>
        <item name="android:popupTheme">@*com.google.android.deskclock:style/ThemeOverlay.Popup</item>
        <item name="*com.google.android.deskclock:popupTheme">@*com.google.android.deskclock:style/ThemeOverlay.Popup</item>
        <item name="*com.google.android.deskclock:popupMenuStyle">@*com.google.android.deskclock:style/Widget.PopupMenu</item>
        <item name="*com.google.android.deskclock:imageButtonStyle">@*com.google.android.deskclock:style/Widget.ImageButton</item>
        <item name="*com.google.android.deskclock:colorPrimary">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.deskclock:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.deskclock:colorAccent">@*com.google.android.deskclock:color/google_blue300</item>
        <item name="*com.google.android.deskclock:colorControlNormal">@*android:color/white</item>
        <item name="*com.google.android.deskclock:colorControlActivated">@*android:color/white</item>
        <item name="*com.google.android.deskclock:colorBackgroundFloating">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="Theme.DeskClock.Licenses" parent="@*com.google.android.deskclock:style/Theme.DeskClock">
        <item name="android:title">@*com.google.android.deskclock:string/menu_item_licenses</item>
        <item name="*com.google.android.deskclock:windowActionBar">true</item>
        <item name="*com.google.android.deskclock:windowNoTitle">false</item>
        <item name="*com.google.android.deskclock:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="Theme.DeskClock.RingtonePicker" parent="@*com.google.android.deskclock:style/Theme.DeskClock">
        <item name="*com.google.android.deskclock:windowActionBar">true</item>
        <item name="*com.google.android.deskclock:windowNoTitle">false</item>
        <item name="*com.google.android.deskclock:actionBarStyle">@*com.google.android.deskclock:style/Widget.ActionBar</item>
        <item name="*com.google.android.deskclock:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="Theme.DeskClock.Screensaver" parent="@*com.google.android.deskclock:style/Theme.DeskClock">
        <item name="android:windowBackground">@*android:color/primary_text_leanback_light</item>
        <item name="*com.google.android.deskclock:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="Theme.DeskClock.SearchActivity" parent="@*com.google.android.deskclock:style/Theme.DeskClock">
        <item name="*com.google.android.deskclock:windowActionBar">true</item>
        <item name="*com.google.android.deskclock:windowNoTitle">false</item>
        <item name="*com.google.android.deskclock:actionBarStyle">@*com.google.android.deskclock:style/Widget.ActionBar.NoTitle</item>
        <item name="*com.google.android.deskclock:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.deskclock:autoCompleteTextViewStyle">@*com.google.android.deskclock:style/Widget.SearchView</item>
    </style>
    <style name="Theme.DeskClock.Settings" parent="@*com.google.android.deskclock:style/Theme.DeskClock">
        <item name="android:textColorSecondary">@*com.google.android.deskclock:color/text_color_google_grey_500</item>
        <item name="*com.google.android.deskclock:windowActionBar">true</item>
        <item name="*com.google.android.deskclock:windowNoTitle">false</item>
        <item name="*com.google.android.deskclock:actionBarStyle">@*com.google.android.deskclock:style/Widget.ActionBar</item>
        <item name="*com.google.android.deskclock:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.google.android.deskclock:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.deskclock:colorAccent">@*com.google.android.deskclock:color/google_blue300</item>
        <item name="*com.google.android.deskclock:colorControlActivated">@*com.google.android.deskclock:color/google_blue300</item>
        <item name="*com.google.android.deskclock:preferenceTheme">@*com.google.android.deskclock:style/PreferenceThemeOverlay.v14.Material</item>
    </style>
    <style name="Widget.Design.BottomSheet.Modal" parent="@*android:style/Widget">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:elevation">@*com.google.android.deskclock:dimen/design_bottom_sheet_modal_elevation</item>
        <item name="*com.google.android.deskclock:behavior_peekHeight">auto</item>
        <item name="*com.google.android.deskclock:behavior_hideable">true</item>
        <item name="*com.google.android.deskclock:behavior_skipCollapsed">false</item>
        <item name="*com.google.android.deskclock:shapeAppearance">@null</item>
        <item name="*com.google.android.deskclock:shapeAppearanceOverlay">@null</item>
        <item name="*com.google.android.deskclock:enforceMaterialTheme">false</item>
        <item name="*com.google.android.deskclock:backgroundTint">@*android:color/background_floating_material_dark</item>
    </style>
</resources>
